            $('body').css('padding-right', scrollbarWidth);
        }

        $rootScope.onFooterCtaNewsletterClick = function ($event) {

            toggleModal();
            $event.preventDefault();
        }

        $rootScope.$on('newsletter_signup_submit', function () {
            $rootScope.footer.isFooterNewsletterSignedUp = true;

        });
    }
    return {
        link: link
    }
