/*
jquery-circle-progress - jQuery Plugin to draw animated circular progress bars

URL: http://kottenator.github.io/jquery-circle-progress/
Author: Rostyslav Bryzgunov <kottenator@gmail.com>
Version: 1.1.3
License: MIT
*/
(function($) {
    function CircleProgress(config) {
        this.init(config);
    }

    CircleProgress.prototype = {
        //----------------------------------------------- public options -----------------------------------------------
        /**
         * This is the only required option. It should be from 0.0 to 1.0
         * @type {number}
         */
        value: 0.0,

        /**
         * Size of the circle / canvas in pixels
         * @type {number}
         */
        size: 100.0,

        /**
         * Initial angle for 0.0 value in radians
         * @type {number}
         */
        startAngle: -Math.PI,

        /**
         * Width of the arc. By default it's auto-calculated as 1/14 of size, but you may set it explicitly in pixels
         * @type {number|string}
         */
        thickness: 'auto',

        /**
         * Fill of the arc. You may set it to:
         *   - solid color:
         *     - { color: '#3aeabb' }
         *     - { color: 'rgba(255, 255, 255, .3)' }
         *   - linear gradient (left to right):
         *     - { gradient: ['#3aeabb', '#fdd250'], gradientAngle: Math.PI / 4 }
         *     - { gradient: ['red', 'green', 'blue'], gradientDirection: [x0, y0, x1, y1] }
         *   - image:
         *     - { image: 'http://i.imgur.com/pT0i89v.png' }
         *     - { image: imageObject }
         *     - { color: 'lime', image: 'http://i.imgur.com/pT0i89v.png' } - color displayed until the image is loaded
         */
        fill: {
            gradient: ['#3aeabb', '#fdd250']
        },

        /**
         * Color of the "empty" arc. Only a color fill supported by now
         * @type {string}
         */
        emptyFill: 'rgba(0, 0, 0, .1)',

        /**
         * Animation config (see jQuery animations: http://api.jquery.com/animate/)
         */
        animation: {
            duration: 1200,
            easing: 'circleProgressEasing'
        },

        /**
         * Default animation starts at 0.0 and ends at specified `value`. Let's call this direct animation.
         * If you want to make reversed animation then you should set `animationStartValue` to 1.0.
         * Also you may specify any other value from 0.0 to 1.0
         * @type {number}
         */
        animationStartValue: 0.0,

        /**
         * Reverse animation and arc draw
         * @type {boolean}
         */
        reverse: false,

        /**
         * Arc line cap ('butt', 'round' or 'square')
         * Read more: https://developer.mozilla.org/en-US/docs/Web/API/CanvasRenderingContext2D.lineCap
         * @type {string}
         */
        lineCap: 'butt',

        //-------------------------------------- protected properties and methods --------------------------------------
        /**
         * @protected
         */
        constructor: CircleProgress,

        /**
         * Container element. Should be passed into constructor config
         * @protected
         * @type {jQuery}
         */
        el: null,

        /**
         * Canvas element. Automatically generated and prepended to the {@link CircleProgress.el container}
         * @protected
         * @type {HTMLCanvasElement}
         */
        canvas: null,

        /**
         * 2D-context of the {@link CircleProgress.canvas canvas}
         * @protected
         * @type {CanvasRenderingContext2D}
         */
        ctx: null,

        /**
         * Radius of the outer circle. Automatically calculated as {@link CircleProgress.size} / 2
         * @protected
         * @type {number}
         */
        radius: 0.0,

        /**
         * Fill of the main arc. Automatically calculated, depending on {@link CircleProgress.fill} option
         * @protected
         * @type {string|CanvasGradient|CanvasPattern}
         */
        arcFill: null,

        /**
         * Last rendered frame value
         * @protected
         * @type {number}
         */
        lastFrameValue: 0.0,

        /**
         * Init/re-init the widget
         * @param {object} config - Config
         */
        init: function(config) {
            $.extend(this, config);
            this.radius = this.size / 2;
            this.initWidget();
            this.initFill();
            this.draw();
        },

        /**
         * @protected
         */
        initWidget: function() {
            var canvas = this.canvas = this.canvas || $('<canvas>').prependTo(this.el)[0];
            canvas.width = this.size;
            canvas.height = this.size;
            this.ctx = canvas.getContext('2d');
        },

        /**
         * This method sets {@link CircleProgress.arcFill}
         * It could do this async (on image load)
         * @protected
         */
        initFill: function() {
            var self = this,
                fill = this.fill,
                ctx = this.ctx,
                size = this.size;

            if (!fill)
                throw Error("The fill is not specified!");

            if (fill.color)
                this.arcFill = fill.color;

            if (fill.gradient) {
                var gr = fill.gradient;

                if (gr.length == 1) {
                    this.arcFill = gr[0];
                } else if (gr.length > 1) {
                    var ga = fill.gradientAngle || 0, // gradient direction angle; 0 by default
                        gd = fill.gradientDirection || [
                            size / 2 * (1 - Math.cos(ga)), // x0
                            size / 2 * (1 + Math.sin(ga)), // y0
                            size / 2 * (1 + Math.cos(ga)), // x1
                            size / 2 * (1 - Math.sin(ga))  // y1
                        ];

                    var lg = ctx.createLinearGradient.apply(ctx, gd);

                    for (var i = 0; i < gr.length; i++) {
                        var color = gr[i],
                            pos = i / (gr.length - 1);

                        if ($.isArray(color)) {
                            pos = color[1];
                            color = color[0];
                        }

                        lg.addColorStop(pos, color);
                    }

                    this.arcFill = lg;
                }
            }

            if (fill.image) {
                var img;

                if (fill.image instanceof Image) {
                    img = fill.image;
                } else {
                    img = new Image();
                    img.src = fill.image;
                }

                if (img.complete)
                    setImageFill();
                else
                    img.onload = setImageFill;
            }

            function setImageFill() {
                var bg = $('<canvas>')[0];
                bg.width = self.size;
                bg.height = self.size;
                bg.getContext('2d').drawImage(img, 0, 0, size, size);
                self.arcFill = self.ctx.createPattern(bg, 'no-repeat');
                self.drawFrame(self.lastFrameValue);
            }
        },

        draw: function() {
            if (this.animation)
                this.drawAnimated(this.value);
            else
                this.drawFrame(this.value);
        },

        /**
         * @protected
         * @param {number} v - Frame value
         */
        drawFrame: function(v) {
            this.lastFrameValue = v;
            this.ctx.clearRect(0, 0, this.size, this.size);
            this.drawEmptyArc(v);
            this.drawArc(v);
        },

        /**
         * @protected
         * @param {number} v - Frame value
         */
        drawArc: function(v) {
            var ctx = this.ctx,
                r = this.radius,
                t = this.getThickness(),
                a = this.startAngle;

            ctx.save();
            ctx.beginPath();

            if (!this.reverse) {
                ctx.arc(r, r, r - t / 2, a, a + Math.PI * 2 * v);
            } else {
                ctx.arc(r, r, r - t / 2, a - Math.PI * 2 * v, a);
            }

            ctx.lineWidth = t;
            ctx.lineCap = this.lineCap;
            ctx.strokeStyle = this.arcFill;
            ctx.stroke();
            ctx.restore();
        },

        /**
         * @protected
         * @param {number} v - Frame value
         */
        drawEmptyArc: function(v) {
            var ctx = this.ctx,
                r = this.radius,
                t = this.getThickness(),
                a = this.startAngle;

            if (v < 1) {
                ctx.save();
                ctx.beginPath();

                if (v <= 0) {
                    ctx.arc(r, r, r - t / 2, 0, Math.PI * 2);
                } else {
                    if (!this.reverse) {
                        ctx.arc(r, r, r - t / 2, a + Math.PI * 2 * v, a);
                    } else {
                        ctx.arc(r, r, r - t / 2, a, a - Math.PI * 2 * v);
                    }
                }

                ctx.lineWidth = t;
                ctx.strokeStyle = this.emptyFill;
                ctx.stroke();
                ctx.restore();
            }
        },

        /**
         * @protected
         * @param {number} v - Value
         */
        drawAnimated: function(v) {
            var self = this,
                el = this.el,
                canvas = $(this.canvas);

            // stop previous animation before new "start" event is triggered
            canvas.stop(true, false);
            el.trigger('circle-animation-start');

            canvas
                .css({ animationProgress: 0 })
                .animate({ animationProgress: 1 }, $.extend({}, this.animation, {
                    step: function (animationProgress) {
                        var stepValue = self.animationStartValue * (1 - animationProgress) + v * animationProgress;
                        self.drawFrame(stepValue);
                        el.trigger('circle-animation-progress', [animationProgress, stepValue]);
                    }
                }))
                .promise()
                .always(function() {
                    // trigger on both successful & failure animation end
                    el.trigger('circle-animation-end');
                });
        },

        /**
         * @protected
         * @returns {number}
         */
        getThickness: function() {
            return $.isNumeric(this.thickness) ? this.thickness : this.size / 14;
        },

        getValue: function() {
            return this.value;
        },

        setValue: function(newValue) {
            if (this.animation)
                this.animationStartValue = this.lastFrameValue;
            this.value = newValue;
            this.draw();
        }
    };

    //-------------------------------------------- Initiating jQuery plugin --------------------------------------------
    $.circleProgress = {
        // Default options (you may override them)
        defaults: CircleProgress.prototype
    };

    // ease-in-out-cubic
    $.easing.circleProgressEasing = function(x, t, b, c, d) {
        if ((t /= d / 2) < 1)
            return c / 2 * t * t * t + b;
        return c / 2 * ((t -= 2) * t * t + 2) + b;
    };

    /**
     * Draw animated circular progress bar.
     *
     * Appends <canvas> to the element or updates already appended one.
     *
     * If animated, throws 3 events:
     *
     *   - circle-animation-start(jqEvent)
     *   - circle-animation-progress(jqEvent, animationProgress, stepValue) - multiple event;
     *                                                                        animationProgress: from 0.0 to 1.0;
     *                                                                        stepValue: from 0.0 to value
     *   - circle-animation-end(jqEvent)
     *
     * @param configOrCommand - Config object or command name
     *     Example: { value: 0.75, size: 50, animation: false };
     *     you may set any public property (see above);
     *     `animation` may be set to false;
     *     you may use .circleProgress('widget') to get the canvas
     *     you may use .circleProgress('value', newValue) to dynamically update the value
     *
     * @param commandArgument - Some commands (like 'value') may require an argument
     */
    $.fn.circleProgress = function(configOrCommand, commandArgument) {
        var dataName = 'circle-progress',
            firstInstance = this.data(dataName);

        if (configOrCommand == 'widget') {
            if (!firstInstance)
                throw Error('Calling "widget" method on not initialized instance is forbidden');
            return firstInstance.canvas;
        }

        if (configOrCommand == 'value') {
            if (!firstInstance)
                throw Error('Calling "value" method on not initialized instance is forbidden');
            if (typeof commandArgument == 'undefined') {
                return firstInstance.getValue();
            } else {
                var newValue = arguments[1];
                return this.each(function() {
                    $(this).data(dataName).setValue(newValue);
                });
            }
        }

        return this.each(function() {
            var el = $(this),
                instance = el.data(dataName),
                config = $.isPlainObject(configOrCommand) ? configOrCommand : {};

            if (instance) {
                instance.init(config);
            } else {
                var initialConfig = $.extend({}, el.data());
                if (typeof initialConfig.fill == 'string')
                    initialConfig.fill = JSON.parse(initialConfig.fill);
                if (typeof initialConfig.animation == 'string')
                    initialConfig.animation = JSON.parse(initialConfig.animation);
                config = $.extend(initialConfig, config);
                config.el = el;
                instance = new CircleProgress(config);
                el.data(dataName, instance);
            }
        });
    };
})(jQuery);

var app = angular.module('beoapp', ['ngTouch', 'ngAnimate', 'angularUtils.directives.dirPagination', 'ngFileUpload'], function () {
    $('select').chosen({ width: '100%', disable_search_threshold: 10 }).on('change', function (evt, params) {
        $(this).next().addClass('chosen-changed');
    });
});

app.controller('default-ctrl', ['$scope', '$rootScope', '$timeout', 'UtilsService', 'beoNewsletterSignupService', function ($scope, $rootScope, $timeout, UtilsService, beoNewsletterSignupService) {
    $scope.frontpageSlideColor = '';
    $rootScope.isLanguageSelectorOpen = false;    
    $rootScope.isFrontpage = $('.frontpage').length > 0;
    $rootScope.isTablet = $('.tablet').length > 0;
    $rootScope.isDesktop = $('.desktop').length > 0;
    $rootScope.isPhone = $('.phone').length > 0;
    $rootScope.isMobileAnchorMenuOpen = false;
    $rootScope.enableUpdatedAnchorMenu = true;
    $rootScope.preventAnchorClick = false;
    $rootScope.overflowHiddenY = false;
    
    // refresh waypoints on image load
    $('.pageContainer img').on('load', function () {
        Waypoint.refreshAll();
    });

    $scope.device = {
        desktop: $('html.desktop').length ? true : false,
        tablet: $('html.tablet').length ? true : false,
        phone: $('html.phone').length ? true : false,
        device: function () {
            if ($('html.desktop').length) {
                return 'desktop';
            }
            if ($('html.tablet').length) {
                return 'tablet';
            }
            if ($('html.phone').length) {
                return 'phone';
            }
            return false;
        }
    };

    $scope.mailto = function (email, subject, body) {
        var link = "mailto:" + email + "?subject=" + escape(subject) + "&body=" + escape(body);
        window.location.href = link;
    };

    // use ng-click="anchorScrollEase($event, '/dir/dir/pagen#my_anchor')" or just
    // ng-click="anchorScrollEase($event, 'my_anchor')"
    // to add scroll animation to internal page anchor links
    $scope.anchorScrollEase = function ($event, href) {
        var anchorStr = href.indexOf('#') > -1 ? href.split('#')[1] : href;
        var $elem;
        var dist;
        var speed;
        var dir;
        var offset;

        if (UtilsService.getIsUrlRelative(href) && anchorStr != undefined) {

            $elem = $('[name=' + anchorStr + ']');

            dist = $elem.offset().top - $($event.currentTarget).offset().top;
            dir = dist < 0 ? 'up' : 'down';

            offset = dir === 'down' ? 0 : ($('[beo-anchor-menu]').length ? $('[beo-anchor-menu]').height() : 0);
            offset = offset + $rootScope.cookieBar.height();

            speed = ((Math.abs(dist) / $(document).height()) * 100) * 80;

            $("html, body").stop().animate({ scrollTop: ($elem.offset().top - offset) }, speed, 'easeInOutCubic');

            $event.preventDefault();
        }
    };

    $rootScope.setNewsletterCookie = function (id) {
        if ($.cookie(id) == undefined) {
            $.cookie(id, 'true', { expires: 7, path: '/' });
        }
    };

    $rootScope.openStoreFinderMap = function ($event) {
        var $target = $($event.currentTarget);

        if ($rootScope.isPhone) {
            window.location.href = $target.attr('href');
        } else {
            $rootScope.openStoreFinderDesktopMap();
            if ($event) {
                $event.preventDefault();
            }
        }
    };

    // DEPRICATED NEWSLETTER SIGNUP STUFF
    // START
    // newsletter signup
    $scope.newsletterFormSubmit = function (evt, cookieid) {
        $rootScope.$broadcast('newsletter_signup_submit', { loading: true, success: null, element: $(evt.currentTarget) });

        $scope.formSent = true;
        $scope.formReceived = true; // disable the form, show button spinner

        beoNewsletterSignupService.sendNewsletterSignup($scope.user, evt.currentTarget).then(function (response) {
            if (response.StatusCode == '200') {
                var scrollToElementPosition = $(evt.currentTarget).offset().top - 70;
                scrollToElementPosition += 'px';

                onNewsletterSignupSuccess(response, cookieid, evt.currentTarget);

                if ($scope.footer) {
                    $scope.footer.email = "";
                }

                $scope.user = {};

                dataLayer.push({
                    'event': 'e',
                    'eName': 'newsletterSignup',
                    'eVar1': $scope.user.nationality
                });

            } else {
                onNewsletterSignupError(response, $(evt.currentTarget));
            }

            if ($rootScope.isPhone) {
                $('html, body').animate({
                    scrollTop: scrollToElementPosition
                });
            }
        });
    };

    // DM Signup
    $scope.newsletterDMFormSubmit = function (evt, cookieid) {
        $rootScope.$broadcast('newsletter_signup_submit', { loading: true, success: null, element: $(evt.currentTarget) });

        $scope.formSent = true;
        $scope.formReceived = true; // disable the form, show button spinner
        //$scope.showSignupLoader = true; // Show spinner

        beoNewsletterSignupService.sendNewsletterDmSignup($scope.dmuser).then(function (response) {
            var scrollToElementPosition = $(evt.currentTarget).offset().top - 70;
            scrollToElementPosition += 'px';

            if (response.StatusCode == '200') {
                onNewsletterSignupSuccess(response, cookieid, evt.currentTarget);

                dataLayer.push({
                    'event': 'e',
                    'eName': 'newsletterDmSignup',
                    'eVar1': $scope.dmuser.nationality
                });
            } else {
                onNewsletterSignupError(response, evt.currentTarget);
            }

            $('html, body').animate({
                scrollTop: (scrollToElementPosition)
            });
        });
    };

    $rootScope.resetNewsletterModalForm = function (resetinp) {
        $scope.formSent = false; // hide the loader
        $scope.showSignupError = false;
        $scope.formHide = false;
        $scope.showSignupLoader = false; // Hide spinner
        $scope.showSignupSuccess = false;
        $scope.formReceived = false;
        $scope.showSignupConfirmed = false;

        if (typeof colorsObj !== "undefined") {
            $scope.newsletterModalBackgroundColor = colorsObj.backgroundColorStep1;
        }

        if (resetinp) {
            $scope.user = {};
        }

        $scope.subscribeNewsletterForm.$setPristine();
        $scope.subscribeNewsletterForm.$setUntouched();
    };

    var onNewsletterSignupSuccess = function (response, cookieid, form) {
        var $modal = $(form).closest('.modal-body');

        $rootScope.$broadcast('newsletter_signup_submit', { loading: false, success: true, element: $(form) });

        if ($modal.length) {
            $scope.newsletterModalHeight = $(form).closest('.modal-body').outerHeight();
        }

        $scope.formSent = false; // hide the loader
        $scope.formHide = true;
        $scope.showSignupLoader = false; // Hide spinner

        $timeout(function () {
            $scope.showSignupSuccess = true;
        }, 50);

        if (cookieid) {
            $rootScope.setNewsletterCookie(cookieid);
        }
    };

    var onNewsletterSignupError = function (response, form) {
        var $modal = $(form).closest('.modal-body');

        $rootScope.$broadcast('newsletter_signup_submit', { loading: false, success: false, element: $(form) });

        $scope.newsletterModalHeight = $modal.outerHeight();
        $scope.formSent = false; // hide the loader
        $scope.showSignupError = true;
        $scope.formHide = true;
        $scope.showSignupLoader = false; // Hide spinner
    };

    // END
    // DEPRICATED NEWSLETTER SIGNUP STUFF

    // not using angular because HTML tag is outside default controller
    $scope.$on('global:updated', function (event, data) {
        if (data.bodyOverflow.value) {
            $('html, body').addClass('overflow-hidden');
        } else {
            $('html, body').removeClass('overflow-hidden');
        }
    });
}]);
app.controller('beo-navigation-main-ctrl', ['$scope', '$element', '$timeout', 'beoScrollToAnchorService', function ($scope, $element, $timeout, beoScrollToAnchorService) {
    $scope.transition = false;

    $scope.fixed = false;
    $scope.isFoldOutOpen = false;    
    $scope.isFoldOutClosing = false;
    $scope.isFoldOutOpening = false;
    $scope.isFoldOutOpened = false;
    $scope.foldOutHeight = $element.find('#nav_foldout_slider .nav__list').outerHeight();
    $scope.foldOutSlider = $element.find('#nav_foldout_slider');
    $scope.mediaQ = '';

    $timeout(function () {
        $scope.transition = true;
        $scope.fixed = true;
    }, 150);
}]);
app.controller('beo-alert', ['$scope', '$attrs', function ($scope, $attrs) {
    //$scope.open = $attrs.open;

    $scope.toggleAlert = function () {
        if ($scope.open) {
            $scope.open = false;
        } else {
            $scope.open = true;

        }
    };
}]);
app.controller('beo-permission-subscribe-ctrl', ['$scope', '$element', 'beoPermissionSubscribeService', function ($scope, $element, beoPermissionSubscribeService) {
    $scope.permissionSignupData = {};
    $scope.permissionUnsubscribeData = {};
    $scope.permissionFormSubitted = false;
    $scope.permissionFormViewHide = false;
    $scope.permissionErrViewShow = false;
    $scope.permissionSuccessViewShow = false;





    // submit permission signup form to signup service
    $scope.permissionSignupFrmSubmit = function ($event, cookieid) {
        $scope.permissionFormSubitted = true;

        // disable chosen country dropdown plugin
        $element.find('.inp__slct').attr('disabled', true).trigger("chosen:updated");

        beoPermissionSubscribeService.submitPermissionSubscribe($scope.permissionSignupData, $event.currentTarget).then(function (response) {

            console.table(response);

            if (response.Status === BangOlufsen.PermissionSignup.ResponseStatus.OK) {
                console.log("OK");
                permissionSignupFrmSubmitSuccess(response, cookieid, $event.currentTarget);
            } else {
                console.log("NOT OK");
                permissionSignupFrmSubmitError(response, $($event.currentTarget));
            }
        });
    }

    $scope.$on('imageLoaded', onPackshotImageLoaded);

    function onPackshotImageLoaded(e, d) {
        $(window).trigger('resize');
    }

    // submit permission signup form success handler
    function permissionSignupFrmSubmitSuccess(response, cookieid, form) {
        // tracking
        dataLayer.push({
            'event': 'e',
            'eName': 'newsletterSignup',
            'eVar1': $scope.permissionSignupData.nationality
        });

        $scope.permissionSignupData = {};
        $scope.permissionFormSubitted = false;
        $scope.permissionFormViewHide = true;
        $scope.permissionSuccessViewShow = true;

        $element.find('.permsignup__slct').attr('disabled', false).trigger("chosen:updated");
    }

    // submit permission signup form error handler
    function permissionSignupFrmSubmitError(response, form) {
        $scope.permissionSignupData = {};
        $scope.permissionFormSubitted = false;
        $scope.permissionFormViewHide = true;
        $scope.permissionErrViewShow = true;

        $element.find('.inp__slct').attr('disabled', false).trigger("chosen:updated");
    }

    //
    // unsubscribe
    //
    $scope.permissionUnsubscribeFrmSubmit = function (evt) {


        var noResonTxt = 'no reason';
        var unsubReasonVal = $scope.permissionSignupData.reasonToUnsubscribe === undefined ? noResonTxt : $scope.permissionSignupData.reasonToUnsubscribe;
        var unsubReasonLabel = $scope.permissionSignupData.reasonToUnsubscribe === undefined ? '' : $('[value="' + $scope.permissionSignupData.reasonToUnsubscribe + '"]').attr('data-label');

        $scope.permissionSignupData.reasonToUnsubscribe = unsubReasonVal;

        $scope.permissionFormSubitted = true;


        beoPermissionSubscribeService.submitPermissionUnsubscribe($scope.permissionSignupData).then(function (response) {

            if (response.Status === BangOlufsen.PermissionSignup.ResponseStatus.OK) {
                permissionUnsubscribeFrmSubmitSuccess(response, evt.currentTarget);


                dataLayer.push({
                    'event': 'e',
                    'eName': 'newsletterUnsubscribe',
                    'eVar1': unsubReasonVal + ': ' + unsubReasonLabel
                });
            } else {
                permissionUnsubscribeFrmSubmitError(response, $(evt.currentTarget));
            }
        });
    };


    // submit permission unsubscribe form success handler
    function permissionUnsubscribeFrmSubmitSuccess(response, cookieid, form) {
        $scope.permissionSignupData = {};
        $scope.permissionFormSubitted = false;
        $scope.permissionFormViewHide = true;
        $scope.permissionSuccessViewShow = true;
    }

    // submit permission unsubscribe form error handler
    function permissionUnsubscribeFrmSubmitError(response, form) {
        $scope.permissionSignupData = {};
        $scope.permissionFormSubitted = false;
        $scope.permissionFormViewHide = true;
        $scope.permissionErrViewShow = true;
    }
}]);
app.controller('beo-mediaserver-activation-form', ['$scope', '$element', function ($scope, $element) {
    // if mediaactivation code is not valid, scroll to input field
    var $activationCodeInp = $element.find('.msam__inp--error');
    if ($activationCodeInp.length > 0) {
        $(document).scrollTop($activationCodeInp.eq(0).offset().top - 180);
    }

    // form validation
    $element.find('form').validate({
        highlight: function (element) {
            $(element).addClass('ng-invalid').closest('form').addClass('ng-submitted');
        },
        unhighlight: function (element) {
            $(element).removeClass('ng-invalid');
        },
        errorPlacement: function (error, element) { }
    });
}]);
/// <reference path="../../Vendor/Angular/angular.min.js" />
/// <reference path="../../Vendor/Jquery/jquery-1.11.3.js" />
app.controller('beo-bs35-signup', ['$scope', '$element', 'UtilsService', function ($scope, $element, UtilsService) {

    var reservationParam = UtilsService.getURLParameter('r');
    var hashParam = UtilsService.getURLParameter('h');
    var idParam = UtilsService.getURLParameter('i');
    var nameParam = UtilsService.getURLParameter('n');
    var emailParam = UtilsService.getURLParameter('e');
    var countryParam = UtilsService.getURLParameter('c');

    $scope.isFormError = false;
    $scope.showBs35SignupCTA = true;
    $scope.showBs35SignupNewsletter = false;
    $scope.showBs35SignupReservation = false;
    $scope.isNewsletterEmailConfirmed = false;
    $scope.isReservationEmailConfirmed = false;
    $scope.isJsonLoadError = false;

    // signup newsletter
    if (typeof hashParam !== 'undefined' && typeof idParam !== 'undefined' && typeof reservationParam === 'undefined') {
        location.hash = 'bs35_signup_cta';

        //Beosound35Service.Newsletter.GetCountries(id, name, email, country, hash, onSuccess, onError);
        Beosound35Service.Newsletter.Confirm(idParam, nameParam, emailParam, countryParam, hashParam, onNewsletterConfirmSuccess, onNewsletterConfirmError);
    }

    // signup reservation
    if (typeof hashParam !== 'undefined' && typeof idParam !== 'undefined' && typeof reservationParam !== 'undefined') {
        //Beosound35Service.Reservation.Confirm(id, hash, onSuccess, onError);
        Beosound35Service.Reservation.Confirm(idParam, hashParam, onReservationConfirmSuccess, onReservationConfirmError);
    }

    function onNewsletterConfirmSuccess(e) {
        $scope.showBs35SignupCTA = false;
        $scope.showBs35SignupNewsletter = true;
        $scope.isNewsletterEmailConfirmed = true;
        //$scope.apply();
        $(window).trigger('resize');
    }

    function onNewsletterConfirmError(e) {
        $scope.showBs35SignupCTA = false;
        $scope.isFormError = true;
        //$scope.apply();
        $(window).trigger('resize');
    }

    function onReservationConfirmSuccess(e) {
        $scope.showBs35SignupCTA = false;
        $scope.showBs35SignupReservation = true;
        $scope.isReservationEmailConfirmed = true;
        //$scope.apply();
        $(window).trigger('resize');
    }

    function onReservationConfirmError(e) {
        $scope.isFormError = true;
        //$scope.apply();
        $(window).trigger('resize');
    }

    $scope.reset = function () {
        $scope.isFormError = false;
        $scope.showBs35SignupCTA = true;
        $scope.showBs35SignupNewsletter = false;
        $scope.showBs35SignupReservation = false;
        $scope.isNewsletterEmailConfirmed = false;
        $scope.isReservationEmailConfirmed = false;
        $scope.isJsonLoadError = false;
    }
}]);
/// <reference path="../../Vendor/Angular/angular.min.js" />
/// <reference path="../../Vendor/Jquery/jquery-1.11.3.js" />
app.controller('beo-bs35-newsletter-signup', ['$scope', '$rootScope', '$element', function ($scope, $rootScope, $element) {
    var updateInterval;
    var countryHasHandler = false;
    var currentCountry = BangOlufsen.Context.Language + '-' + BangOlufsen.Context.OriginCountry;

    $scope.isFormSubmitted = $scope.$parent.isNewsletterEmailConfirmed ? true : false;
    $scope.formdata = {};
    $scope.showLoader = false;
    $scope.countries = [];

    Beosound35Service.Newsletter.GetCountries(onCountriesLoadedSuccess);

    toggleDisabled('Bs35NewsletterSignupCountry', true);
    toggleLoading('Bs35NewsletterSignupCountry', true);

    $scope.submit = function () {
        if ($scope.bs35_newsletter_signup_frm.$valid) {
            //Submit: function (firstname, lastname, country, email, onSuccess, onError)
            Beosound35Service.Newsletter.Submit($scope.formdata.firstname, $scope.formdata.lastname, $scope.formdata.country.Code, $scope.formdata.email, onSubmitSuccess, onSubmitError);
            $scope.showLoader = true;
        } else {
            console.log('form not valid!');
        }
    }

    $scope.$watch('$parent.showBs35SignupCTA', function (newValue, oldValue) {
        if (newValue) {
            $element.find('input[type="text"]').val('');
        }
    });

    function getScrollToPosition() {
        return $element.position().top - ($('.phone').length ? 0 : 100);
    }

    function onSubmitSuccess(e) {
        if (e.Success) {
            console.log('success', e);

            $scope.showLoader = false;
            $scope.isFormSubmitted = true;
            $scope.$apply();

            $('html, body').animate({ scrollTop: getScrollToPosition() }, 500);

            if (typeof dataLayer !== 'undefined') {
                dataLayer.push({
                    'event': 'e',
                    'eName': 'infoSignup',
                    'eVar1': BangOlufsen.Context.OriginCountry
                });
            }
            //debugger;
        } else {
            onSubmitError(e);
        }
    }

    function onSubmitError(e) {
        console.log('error', e);
        $scope.showLoader = false;
        $scope.$parent.isFormError = true;
        $scope.$apply();

        setTimeout(function () { $('html, body').animate({ scrollTop: getScrollToPosition() }, 500); }, 100);
    }

    function getIndexFromCountryCode(obj, code) {
        for (var i = 0; i < obj.length; i++) {
            var v = obj[i];
            if (v.Code.toLowerCase() == code.toLowerCase()) {
                return i;
            }
        }
        for (var i = 0; i < obj.length; i++) {
            var v = obj[i];
            var tempCountryISOArr = v.Code.split('-');
            var tempCountryISO = tempCountryISOArr[tempCountryISOArr.length - 1].toLowerCase();
            var countryISOArr = code.split('-');
            var countryISO = countryISOArr[countryISOArr.length - 1].toLowerCase();
            if (tempCountryISO == countryISO) {
                return i;
            }
        }
        return null;
    }

    // countries loaded
    function onCountriesLoadedSuccess(countries) {
        var selectedIndex = getIndexFromCountryCode(countries, currentCountry); // 'en-GB'
        $scope.countries = countries;
        $scope.formdata.country = countries[selectedIndex];

        // this is a hack because of very slow angular rendering
        updateIntervalHandler(countries);
        updateInterval = setInterval(function () { updateIntervalHandler(countries) }, 200);
        $scope.$apply();
    }

    function updateIntervalHandler(countries) {
        if ($('#Bs35NewsletterSignupCountry option').length == countries.length) {

            $('#Bs35NewsletterSignupCountry').trigger("chosen:updated");

            if (!countryHasHandler) {
                $('#Bs35NewsletterSignupCountry').on('change', onCountryChange);
            }

            toggleDisabled('Bs35NewsletterSignupCountry', false);
            toggleLoading('Bs35NewsletterSignupCountry', false);
            $('#Bs35NewsletterSignupCountry_chosen').addClass('chosen-changed');

            clearInterval(updateInterval);
        }
    }

    function onCountryChange(countries) {
        if ($('.phone').length) {
            $scope.formdata.country = { Code: $('#Bs35NewsletterSignupCountry').val() };
        } else {
            $scope.formdata.country = { Code: $('#Bs35NewsletterSignupCountry').chosen().val(), Name: $('#Bs35NewsletterSignupCountry :selected').text() };
        }
        //$scope.$apply();
    }

    function resetDropdown(id) {
        var $elem = $('#' + id);
        $elem.find('option').remove();
        $scope.bs35_reservation_signup_frm[id].$setPristine();
        $scope.bs35_reservation_signup_frm.$setPristine();

        $elem.trigger("chosen:updated");
    }

    function toggleDisabled(id, disabled) {
        var $elem = $('#' + id);

        if (disabled) {
            $elem.attr('disabled', 'disabled');
        } else {
            $elem.removeAttr('disabled', 'disabled');
        }

        $elem.trigger("chosen:updated");
    }

    function toggleLoading(id, show) {
        var $elem = $('#' + id);
        if (show) {
            $elem.siblings('.spinner').show();
        } else {
            $elem.siblings('.spinner').hide();
        }
    }
}]);

/// <reference path="../../Vendor/Angular/angular.min.js" />
/// <reference path="../../Vendor/Jquery/jquery-1.11.3.js" />
app.controller('beo-bs35-reservation-signup', ['$scope', '$rootScope', '$element', function ($scope, $rootScope, $element) {
    $scope.isFormSubmitted = $scope.$parent.isReservationEmailConfirmed ? true : false;
    $scope.showLoader = false;
    $scope.formdata = {};
    $scope.countries = [];
    $scope.cities = [];
    $scope.stores = [];

    $scope.country = '';
    $scope.city = '';
    $scope.store = '';
    $scope.storeid = '';

    var countryHasHandler = false;
    var cityHasHandler = false;
    var storeHasHandler = false;
    var updateCountriesInterval;
    var updateCitiesInterval;
    var updateStoresInterval
    var scrollToOnSubmit = $element.position().top - ($('.phone').length ? 0 : 100);

    toggleDisabled('Bs35ReservationSignupCountry', true);
    toggleLoading('Bs35ReservationSignupCountry', true);
    toggleDisabled('Bs35ReservationSignupCity', true);
    toggleDisabled('Bs35ReservationSignupStore', true);

    Beosound35Service.Reservation.GetCountries(onCountriesLoadedSuccess);

    $scope.submit = function () {

        if ($scope.bs35_reservation_signup_frm.$valid) {
            //Reserve: function (firstname, lastname, country, email, storeId, city, units, onSuccess, onError)
            Beosound35Service.Reservation.Reserve($scope.formdata.firstname, $scope.formdata.lastname, $scope.formdata.country.Code, $scope.formdata.email, $scope.formdata.store, $scope.formdata.city.City, $scope.formdata.amount, onSubmitSuccess, onSubmitError);

            $scope.showLoader = true;
        } else {
            console.log('form not valid!');
        }
    }

    $scope.$watch('$parent.showBs35SignupCTA', function (newValue, oldValue) {
        if (newValue) {
            $element.find('input[type="text"]').val('');
        }
    });

    function onSubmitSuccess(e) {
        if (e.Success) {
            $scope.showLoader = false;
            $scope.isFormSubmitted = true;

            $scope.$apply();

            $('html, body').animate({ scrollTop: getScrollToPosition() }, 500);

            if (typeof dataLayer !== 'undefined') {
                dataLayer.push({
                    'event': 'e',
                    'eName': 'productReservation',
                    'eVar1': BangOlufsen.Context.OriginCountry + '|' + $scope.formdata.city + '|' + $scope.formdata.store + '|' + $scope.formdata.amount
                });
            }
        } else {
            onSubmitError(e);
        }
    }

    function onSubmitError(e) {
        console.log('error', e);
        $scope.showLoader = false;

        $scope.$parent.isFormError = true;
        $scope.$apply();

        setTimeout(function () { $('html, body').animate({ scrollTop: getScrollToPosition() }, 500); }, 100);
    }

    function getIndexFromCountryCode(obj, code) {

        for (var i = 0; i < obj.length; i++) {
            var v = obj[i];
            if (v.Code.toLowerCase() == code.toLowerCase()) {
                return i;
            }
        }
        return null;
    }

    function getScrollToPosition() {
        return $element.position().top - ($('.phone').length ? 0 : 100);
    }

    // countries loaded
    function onCountriesLoadedSuccess(countries) {
        var selectedIndex = getIndexFromCountryCode(countries, BangOlufsen.Context.OriginCountry);

        $scope.countries = countries;
        $scope.formdata.country = countries[selectedIndex];


        $('#Bs35ReservationSignupCountry_chosen').addClass('chosen-changed');

        // this is a hack because of very slow angular rendering
        updateCountriesIntervalHandler(countries);
        updateCountriesInterval = setInterval(function () { updateCountriesIntervalHandler(countries, selectedIndex); }, 200);

        $scope.$apply();
    }

    function updateCountriesIntervalHandler(countries, selectedIndex) {
        if ($('#Bs35ReservationSignupCountry option').length - 0 == countries.length) {
            if (!countryHasHandler) {
                countryHasHandler = true;

                onCountryChange(countries)
                $('#Bs35ReservationSignupCountry').on('change', onCountryChange);
            }

            toggleDisabled('Bs35ReservationSignupCountry', false);
            toggleLoading('Bs35ReservationSignupCountry', false);

            $('#Bs35ReservationSignupCountry').trigger("chosen:updated");
            clearInterval(updateCountriesInterval);
        }
    }

    function onCountryChange(countries) {
        if ($('.phone').length) {
            $scope.country = { Code: $('#Bs35ReservationSignupCountry').val() };
        } else {
            $scope.country = { Code: $('#Bs35ReservationSignupCountry').chosen().val() };
            $scope.$apply();
        }

        //$scope.formdata = {};
        $scope.formdata.country = $scope.country;

        $scope.formdata.city = null;
        $scope.formdata.store = null;
        $scope.cities = [];
        $scope.stores = [];
        $scope.city = '';
        $scope.store = '';
        $scope.storeid = '';


        resetDropdown('Bs35ReservationSignupCity');
        resetDropdown('Bs35ReservationSignupStore');
        toggleDisabled('Bs35ReservationSignupCity', true);
        toggleDisabled('Bs35ReservationSignupStore', true);
        toggleLoading('Bs35ReservationSignupCity', true);

        Beosound35Service.Reservation.GetCities($scope.country.Code, onCitiesLoadedSuccess);
    }

    // cities loaded
    function onCitiesLoadedSuccess(cities) {

        $scope.cities = cities;

        // this is a hack because of very slow angular rendering
        updateCitiesIntervalHandler(cities);
        updateCitiesInterval = setInterval(function () { updateCitiesIntervalHandler(cities); }, 200);
        $scope.$apply();
    }

    function updateCitiesIntervalHandler(cities) {
        if ($('#Bs35ReservationSignupCity option').length - 1 == cities.length) {
            if (!cityHasHandler) {
                cityHasHandler = true;
                $('#Bs35ReservationSignupCity').on('change', onCityChange);
            }

            toggleDisabled('Bs35ReservationSignupCity', false);
            toggleLoading('Bs35ReservationSignupCity', false);

            $('#Bs35ReservationSignupCity').trigger("chosen:updated");
            clearInterval(updateCitiesInterval);
        }
    }

    function onCityChange(e) {
        $scope.store = '';
        $scope.storeid = '';

        if ($('.phone').length) {
            $scope.city = $('#Bs35ReservationSignupCity').val();
        } else {
            $scope.city = $('#Bs35ReservationSignupCity').chosen().val();
            $scope.$apply();
        }

        toggleDisabled('Bs35ReservationSignupStore', true);
        toggleLoading('Bs35ReservationSignupStore', true);
        resetDropdown('Bs35ReservationSignupStore');

        Beosound35Service.Reservation.GetStores($scope.country.Code, $scope.city, onStoresLoadedSuccess);
    }

    // stores loaded
    function onStoresLoadedSuccess(stores) {
        $scope.stores = stores;

        // this is a hack because of very slow angular rendering
        updateStoresIntervalHandler(stores);
        updateStoresInterval = setInterval(function () { updateStoresIntervalHandler(stores); }, 200);

        //if (!$('.phone').length) {
        $scope.$apply();
        //}
    }

    function updateStoresIntervalHandler(stores) {
        if ($('#Bs35ReservationSignupStore option').length - 1 == stores.length) {
            if (!storeHasHandler) {
                storeHasHandler = true;
                $('#Bs35ReservationSignupStore').on('change', onStoreChange);
            }

            toggleDisabled('Bs35ReservationSignupStore', false);
            toggleLoading('Bs35ReservationSignupStore', false);
            $('#Bs35ReservationSignupStore').trigger("chosen:updated");

            clearInterval(updateStoresInterval);
        }
    }

    function onStoreChange(e) {
        if ($('.phone').length) {
            $scope.store = $('#Bs35ReservationSignupStore').val();
        } else {
            $scope.store = $('#Bs35ReservationSignupStore').chosen().val();
            $scope.$apply();
        }

        $scope.formdata.store = $scope.store;
    }

    function resetDropdown(id) {
        var $elem = $('#' + id);
        $elem.find('option').remove();
        $scope.bs35_reservation_signup_frm[id].$setPristine();
        $scope.bs35_reservation_signup_frm.$setPristine();

        $elem.trigger("chosen:updated");
    }

    function toggleDisabled(id, disabled) {
        var $elem = $('#' + id);

        if (disabled) {
            $elem.attr('disabled', 'disabled');
        } else {
            $elem.removeAttr('disabled', 'disabled');
        }

        $elem.trigger("chosen:updated");
    }

    function toggleLoading(id, show) {
        var $elem = $('#' + id);
        if (show) {
            $elem.siblings('.spinner').show();
        } else {
            $elem.siblings('.spinner').hide();
        }
    }
}]);
/// <reference path="../../Vendor/Angular/angular.min.js" />
/// <reference path="../../Vendor/Jquery/jquery-1.11.3.js" />
app.controller('beo-bs35-dragger', ['$scope', '$element', 'UtilsService', function ($scope, $element, UtilsService) {
    var $images = $element.find('.bs35-dragger-content-image-wrp img');
    var $hotspots = $element.find('.bs35-dragger-content-image-wrp .hotspot');
    var openHotspots = 0;
    $scope.onBs35HotspotClick = function (event) {
        setTimeout(function () {
            var me = $(event.currentTarget);
            openHotspots = me.hasClass('open') ? openHotspots + 1 : openHotspots - 1;
            toggleAltImage(openHotspots > 0 ? true : false);
        }, 50);
    }

    function toggleAltImage(show) {
        if (show) {
            $images.eq(1).addClass('active');
        } else {
            $images.eq(1).removeClass('active');
        }
    }
}]);
