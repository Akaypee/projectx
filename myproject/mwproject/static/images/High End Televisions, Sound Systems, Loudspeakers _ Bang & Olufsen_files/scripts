app.controller('beo-doc-download-ctrl', ['$scope', '$rootScope', 'UtilsService', function ($scope, $rootScope, UtilsService) {
    $('#divider').height($('.nav-pills').height());

    $('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
        var targetButton = $(e.target).data('target').replace('#', '');

        $('.bottonMore').hide();

        $('#buttonMore-' + targetButton).show();
    });

    $('.bottonMore').on('click', function (e) {
        var targetCollapse = $('#' + e.target.id).data('collapse');

        $(targetCollapse).collapse('toggle');

        $('#' + e.target.id).attr('id', '')

        $(e.target).hide();

        $('#' + e.target.id).fadeOut();

        $(targetCollapse).on('shown.bs.collapse', function () { });
    });
}]);
