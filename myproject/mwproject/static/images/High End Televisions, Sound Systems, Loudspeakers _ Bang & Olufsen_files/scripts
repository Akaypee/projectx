app.controller('beo-newsletter-signup-ctrl', ['$scope', '$element', 'beoNewsletterSignupService', 'beoNewsletterUnsubscribeService', '$rootScope', function ($scope, $element, beoNewsletterSignupService, beoNewsletterUnsubscribeService, $rootScope) {

    $rootScope.$watch('footer.email', function (newval, oldval) {
     //   $scope.user.email = newval;
    });



    //$rootScope.resetNewsletterModalForm = function () {
    //    $scope.formSent = false; // hide the loader
    //    $scope.showSignupError = false;
    //    $scope.formHide = false;
    //    $scope.showSignupLoader = false; // Hide spinner
    //    $scope.showSignupSuccess = false;
    //    $scope.formReceived = false;

    //    $scope.subscribeNewsletterForm.$setPristine();
    //    $scope.subscribeNewsletterForm.$setUntouched();
    //};

    //$scope.displayForm = function (element) {
    //    $('#collapseOpenButton').fadeOut('fast', myTimelineRun());
    //    function myTimelineRun() {
    //        var tl = new TimelineMax({ delay: 0.2 }).timeScale(0.5);
    //        tl.to($('.row-one'), 1, { autoAlpha: 1, display: 'block' });
    //        tl.to($('.row-two'), 1, { autoAlpha: 1, display: 'block' }, "-=0.7");
    //        tl.to($('.row-three'), 1, { autoAlpha: 1, display: 'block' }, "-=0.7");
    //        tl.to($('#registerButton'), 1, { autoAlpha: 1, display: 'block' }, "-=0.7");
    //    }
    //}

    //$scope.newsletterFormSubmit = function (evt, cookieid) {
    //    console.log('newsletterFormSubmit');

    //    $rootScope.$broadcast('newsletter_signup_submit', { loading: true, success: null, element: $element });

