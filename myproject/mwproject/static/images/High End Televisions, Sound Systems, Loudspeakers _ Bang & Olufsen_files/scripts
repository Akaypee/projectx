app.directive('beoFooterFindStoreCta', [function () {
    function link(scope, element, attr) {
        var $frm = element.find('#footer_cta_findstore_frm');
        var $submit = element.find('#footer_cta_findstore_sbmt');
        var $input = element.find('#footer_cta_findstore_inp');

        scope.isFindStoreSubmitted = false;
        scope.inpFindStoreVal;

        scope.onFooterCtaFindStoreKeyUp = function ($event) {
            var $target = $($event.currentTarget);
            var key = $event.which;

            scope.isSubmitted = false;

            if (key == 13) {
                submitFindStore($input.val());
            }
        }

        scope.onFooterCtaFindStoreSubmit = function ($event) {
            $event.preventDefault();
        }

        scope.onFooterCtaFindStoreClick = function ($event) {
            submitFindStore($input.val());
        }

        function submitFindStore(val) {
            if (val.length > 0) {
                $input.removeClass('footer__ctainp--invalid');

                redirectToFindStore(val);
            } else {
                $input.addClass('footer__ctainp--invalid');
            }
        }

        function redirectToFindStore(val) {
            setTimeout(function () {
                var href = $frm.attr('action');
                var q;

                scope.isFindStoreSubmitted = false;

                if (BangOlufsen.Context.OriginCountry != 'CN') {
                    q = href.indexOf('?') > -1 ? '&' : '?';
                    if (typeof val != 'undefined' && val != '') {
                        href = href + q + 'findstore=' + encodeURI(val);
                        window.location.href = href;
                    } else {
                        scope.isFindStoreSubmitted = true;
                    }
                }
            }, 50);
        }
    }

    return {
        link: link
    }
}]);
app.directive('beoSquare', [function () {
    function link(scope, element, attr) {
        var breakpoint = attr['breakpoint'];

        $(window).on('resize', function (e) {
            if (breakpoint) {
                if ($(window).outerWidth() < breakpoint) {
                    $(element).height($(element).width());
                } else {
                    $(element).removeAttr('style');
                }
            } else {
                $(element).height($(element).width());
            }
        }).trigger('resize');
    }
    return {
        link: link
    }
}]);
app.directive('beoGoogleMapsSuggestions', ['UtilsService', function (UtilsService) {
    function link(scope, element, attr) {
        var input = document.getElementById(element.attr('id'));
        var $pacContainers = $('.pac-container');
        var isAutoCompleteReadyInterval;

        var options = {
            componentRestrictions: { country: BangOlufsen.Context.OriginCountry }
        };

        window.onload = function () {
            var input = document.getElementById(element.attr('id'));
            var place = '';
            autocomplete = new google.maps.places.Autocomplete(input, options);

            autocomplete.addListener('place_changed', function () {
                place = autocomplete.getPlace().formatted_address;

                scope.inpFindStoreVal = autocomplete.getPlace().formatted_address;
            });

            // this is bad, but google places does not add ID's to pac container
            isAutoCompleteReadyInterval = setInterval(function () {
                if ($('.pac-container').length) {
                    clearInterval(isAutoCompleteReadyInterval);
                    isAutoCompleteReadyInterval = null;

                    $('.pac-container').eq($pacContainers.length - 1).addClass('footer__pac');
                }
            }, 250);
        }

    }
    return {
        link: link
    }
}]);
app.directive('beoHeroVideoPlayer', ['$timeout', '$rootScope', function ($timeout, $rootScope) {
    function link(scope, element, attr) {
        var $copy = element.find('.hrvidplr__copy');
        var resizeTimer = null;
        var $videosWrp = element.find('.hrvidplr__videos');
        var minimizeTimer = null;
        var isPlayTracked = false;
        var is10SecTracked = false;

        scope.$watch('forceHeroPVideoPlayerMinimize', function () {

        });

        $rootScope.hideTopBar = false;

        var video = (function () {
            var vids = {};
            element.find('.hrvidplr__video').each(
                function (index) {
                    var id = $(this).attr('data-video-type');
                    var type = id + 'video';
                    vids[type] = videojs($(this).attr('id'));
                });
            return vids;
        })();

        scope.isResizing = false;
        scope.isMainVideoReady = false;
        scope.isTeaserVideoReady = true;
        scope.isVideoPlayerOpen = false;
        scope.cookieAccepted = false;

        scope.onOpenClick = function ($event) {
            if (scope.isMainVideoReady) {

                var videoFilenameArr = video.mainvideo.cache_.src.split('/');
                var videoFilename = videoFilenameArr[videoFilenameArr.length - 1];
                var fullScreenVid = element.find('video');

                scope.isVideoPlayerOpen = true;

                $rootScope.hideTopBar = true;
                $rootScope.setToggleSpeed = true;

                // play main video

                video.mainvideo.play();
                video.teaservideo.pause();

                // Video events
                video.mainvideo.on('play', function () {
                    element.find('.vjs-play-control').focus();
                    if (!isPlayTracked) {
                        isPlayTracked = true;

                        if (typeof dataLayer !== 'undefined') {
                            dataLayer.push({
                                'event': 'e',
                                'eName': 'videoStarted',
                                'eVar1': videoFilename
                            });
                        }
                    }
                });

                video.mainvideo.on('timeupdate', function () {
                    if (fullScreenVid[0].currentTime > 10 && !is10SecTracked) {
                        is10SecTracked = true;

                        if (typeof dataLayer !== 'undefined') {
                            dataLayer.push({
                                'event': 'e',
                                'eName': 'video10sec',
                                'eVar1': videoFilename
                            });
                        }
                    }
                });

                video.mainvideo.on('ended', function () {
                    isPlayTracked = false;

                    if (typeof dataLayer !== 'undefined') {
                        dataLayer.push({
                            'event': 'e',
                            'eName': 'videoEnded',
                            'eVar1': videoFilename
                        });
                    }
                });

                // scroll to top
                $("html, body").animate({
                    scrollTop: 0
                },
                    1000
                );

                // fade out copy
                $copy.on('webkitTransitionEnd otransitionend oTransitionEnd msTransitionEnd transitionend', onCopyFadeoutComplete);

                onMinimizeTimer();

                setTimeout(function () {
                    Waypoint.refreshAll();
                }, 1000);

                $event.preventDefault();
            }
        };

        scope.onCloseClick = function ($event) {
            closeVideoPlayer();

            $event.preventDefault();
        };

        function onCopyFadeoutComplete(e) {
            $copy.css('display', 'none');
            $copy.off('webkitTransitionEnd otransitionend oTransitionEnd msTransitionEnd transitionend', onCopyFadeoutComplete);
        }

        $(window).on('resize', onWindowResize).trigger('resize');


        function onWindowResize() {
            if (!scope.isResizing) {
                scope.isResizing = true;
                scope.$applyAsync();
            }

            $timeout.cancel(resizeTimer);
            resizeTimer = null;
            resizeTimer = $timeout(onWindowResizeTimer, 2000);
        }

        function onWindowResizeTimer() {
            if (scope.isResizing) {
                scope.isResizing = false;
                scope.$applyAsync();
            }
        }

        function buildVideos() {
            video.teaservideo.ready(function () {
                onTeaserVideoReady(this);
            });

            video.mainvideo.ready(function () {
                onMainVideoReady(this);
            });

            video.mainvideo.on('ended', onMainVideoEnd);
        }

        function onTeaserVideoReady(e) {
            if (!scope.forceHeroPVideoPlayerMinimize) {
                minimizeTimer = setTimeout(onMinimizeTimer, 4000);
            } else {
                onMinimizeTimer(true);
            }

            scope.isTeaserVideoReady = true;
        }

        function onMinimizeTimer(force) {
            clearTimeout(minimizeTimer);
            minimizeTimer = null;

            if (force) {
                $videosWrp.removeClass('hrvidplr__videos--transition');

                $videosWrp.addClass('hrvidplr__videos--minimized');

                onCssTransitionMinimize();
            } else {
                $videosWrp.one('webkitTransitionEnd otransitionend oTransitionEnd msTransitionEnd transitionend', onCssTransitionMinimize);

                $videosWrp.addClass('hrvidplr__videos--minimized');
            }
        }

        function onCssTransitionMinimize() {
            $videosWrp.off('webkitTransitionEnd otransitionend oTransitionEnd msTransitionEnd transitionend', onCssTransitionMinimize);

            Waypoint.refreshAll();

            $(window).trigger('scroll').trigger('resize');
        }

        function onMainVideoReady(e) {
            scope.isMainVideoReady = true;
        }

        function onMainVideoEnd(e) {
            closeVideoPlayer();

            scope.$apply();
        }

        function closeVideoPlayer() {
            $rootScope.hideTopBar = false;

            $copy.css('display', '');

            video.teaservideo.play();

            if (!video.mainvideo.ended()) {
                video.mainvideo.pause();
                video.mainvideo.currentTime(0);
            }

            setTimeout(function () {
                onCssTransitionMinimize();
            }, 1500);

            scope.isVideoPlayerOpen = false;
        }

        function onCookieBarAccepted(e) {
            scope.cookieAccepted = true;
        }

        // offsetting closebutton according to cookiebar height if cookies are not accepted
        $rootScope.$on('cookie_accepted', onCookieBarAccepted);

        if ($.cookie('cookie_policy_accepted') !== 'true') {
            scope.cookieAccepted = false;
        } else {
            scope.cookieAccepted = true;
        }


        buildVideos();
    }

    return {
        restrict: 'E',
        link: link,
        controler: 'beo-hero-video-player-ctrl'
    };
}]);
app.directive('beoHeroVideoPlayerMobile', ['$timeout', '$rootScope', function ($timeout, $rootScope) {
    function link(scope, element, attr) {
        var video = (function () {
            var vids = {};
            element.find('.hrvidplr__video').each(
                function (index) {
                    var id = $(this).attr('data-video-type');
                    var type = id + 'video';
                    vids[type] = document.getElementById($(this).attr('id'));
                });
            return vids;
        })();
        
        scope.onOpenClick = function ($event) {
            // play main video
            video.mainvideo.play();

            $event.preventDefault();
        }
    }

    return {
        restrict: 'E',
        link: link
    }
}]);
app.directive('beoCenterAlignMedia', ['$timeout', function ($timeout) {
    function link(scope, element, attr) {
        var mediaQueryBreakPoint = 1022 - 15 // scrollbar;
        var mediaType = element.find('video').length ? 'video' : 'image';
        var isVideoJs = mediaType == 'video' && element.find('.video-js').length ? true : false;
        var $video = element.find('video');
        var initMediaDimensions = { width: 0, height: 0, ratio: 0 };
        var windowRatio = $(window).innerHeight() / $(window).innerWidth();
        var video;
        function runImageResize() { }

        function runVideoResize() {
            video = videojs(element.find('.video-js').attr('id'));

            video.ready(function (e) {
                initMediaDimensions.width = Number(this.tagAttributes.width);
                initMediaDimensions.height = Number(this.tagAttributes.height);
                initMediaDimensions.ratio = initMediaDimensions.height / initMediaDimensions.width;
                
                element.find('.video-js').css({ 'width': '', 'height': '' });

                $(window).on('resize', onWindowResize).trigger('resize');
            });
        }

        function onWindowResize(e) {
            windowRatio = $(window).innerHeight() / $(window).innerWidth();

            if (mediaType == 'video') {
                resizeVideo();
            } else {
                resizeImage();
            }
        }

        function resizeVideo(e) {
            var elemHeight = element.parent().innerHeight();
            var elemWidth = element.parent().innerWidth();
            var videoHeight = $(window).width() * (initMediaDimensions.height / initMediaDimensions.width);
            var videoWidth = $(window).height() * (initMediaDimensions.width / initMediaDimensions.height);
            var margLeft;
            var margTop;

            if ($(window).width() >= mediaQueryBreakPoint) {
                if (windowRatio > initMediaDimensions.ratio) {
                    $video.css({ 'width': 'auto', 'height': '100vh' });

                    margLeft = -1 * ((videoWidth - elemWidth) / 2);

                    $video.css({ 'margin-left': margLeft + 'px', 'margin-top': '' });
                } else {
                    $video.css({ 'width': '100vw', 'height': 'auto' });

                    margTop = -1 * ((videoHeight - elemHeight) / 2);

                    $video.css({ 'margin-left': '', 'margin-top': margTop + 'px' });
                }
            } else {
                $video.css({ 'width': '100%', 'height': 'auto', 'margin-left': '', 'margin-top': '' });
            }
        }

        function resizeImage(e) { }

        if (mediaType == 'video') {
            runVideoResize();
        } else {
            runImageResize();
        }
    }

    return {
        restrict: 'A',
        link: link
    }
}]);
app.directive('beoDiasShow', ['$interval', '$timeout', function ($interval, $timeout) {
    function link(scope, element, attr) {
        var $images = element.find('img');
        var interval = null;
        var c = 0;
        var speed = Number(attr.speed) * 1000;
        var crossfadeSpeed = attr.transitionSpeed < 0 ? 0.1 : attr.transitionSpeed;
        
        function run() {
            onInterval();            
        }

        function onInterval() {
            c = c < $images.length - 1 ? c + 1 : 0;

            $images.eq(c).css('z-index', '1');

            TweenMax.to($images.eq(c), crossfadeSpeed, { alpha: 1, onComplete: runNextSlide });
        }

        function runNextSlide(e) {
            onShowSlideComplete();

            setTimeout(onInterval, speed);
        }

        function onShowSlideComplete() {
            var $t = $images.eq(c);
            var $tprev = $t.prev();
            var tindex = $t.index();

            if (tindex == 0) {
                $tprev = $images.eq($images.length - 1);
            }

            $tprev.removeAttr('style');

            $t.css('z-index', '');
        }

        run();
    }

    return {
        restrict: 'E',
        link: link
    }
}]);
app.directive('beoImageAccordion', ['$interval', '$timeout', function ($interval, $timeout) {
    function link(scope, element, attr) {
        var $element = element;
        var $accordion = element.find('.imgacc__accordion');
        var $mediaWrappers = element.find('.imgacc__mediawrp');
        var $defaultImage = element.find('.imgacc__accimg--default');
        var $prevMediaElem = null;
        var color = attr['class'].indexOf('--black') > -1 ? 'black' : 'white';
        var $activeMediaOld;
        var fadeMedia = attr['fadeMedia'] == 'false' ? false : true;
        var ignoreTabMedia = attr['ignoreTabMedia'] === 'true';
        var isToggleBacgroundColor = attr.toggleBackgroundColor == 'true' ? true : false;
        var fadeOutSpeed = 0.75;
        var changeBgColorSpeed = 1.0;
        var fadeInSpeed = 0.75;
        var playFirstVideoInFirstTab = attr.firstTabPlayFirst == 'true' ? true : false;
        var customBackgroundColor = attr.style != undefined ? element.css('background-color') : undefined;
        var $fullscreenTabImages = element.find('.imgacc__fullscreenimg');
        var fullscreenImages = [];
        var $activeMedia = $fullscreenTabImages.length ? element.find('> picture') : $mediaWrappers.eq(0);

        $accordion.on('show.bs.collapse', onCollapseShow).on('shown.bs.collapse', onCollapseShown).on('hide.bs.collapse', onCollapseHide).on('hidden.bs.collapse', onCollapseHidden);

        var c = 0;
        function loadFullscreenImages() {

            $fullscreenTabImages.each(function (e) {
                var $elem = $(this);
                var url = $(this).attr('data-image-url');

                var tmpImg = new Image();

                $(tmpImg).one('load', function (e) {
                    $elem.closest('.imgacc__fullscreenimg').addClass('imgacc__fullscreenimg--loaded');
                    $elem.find('span').hide();

                    if (c === $fullscreenTabImages.length - 1) {

                        fullscreenImages[0] = element.find('> picture');

                        $fullscreenTabImages.each(function () {
                            fullscreenImages.push($(this));
                        });
                    }
                    c++;
                });
                tmpImg.src = url;

            });
        }

        // on boostrap accordion collapse open/show
        function onCollapseShow(e) {
            var $openTab, $activeMediaWrapper, color, id;
            if (!ignoreTabMedia) {
                // HACK. Do not remove timeout!
                setTimeout(function () {
                    $openTab = $accordion.find('a[aria-expanded="true"]').closest('.panel');
                    id = $openTab.index() + 1;

                    if ($openTab.length) {

                        toggleActiveMedia(id, function () {
                            // if tab media is video then play video
                            if ($activeMedia.find('video').length) {
                                playTabVideo($activeMedia);
                            }
                        });
                    }
                }, 0);
            }
        }

        function onCollapseShown(e) {
            Waypoint.refreshAll();
        }


        // on boostrap accordion collapse close/hide
        function onCollapseHide(e) {
            var $openTab, color;

            if (!ignoreTabMedia) {
                setTimeout(function () {
                    $openTab = $accordion.find('a[aria-expanded="true"]').closest('.panel');

                    if (!$openTab.length) {
                        toggleActiveMedia(0);
                    }
                }, 0);
            }
        }

        function onCollapseHidden(e) {
            Waypoint.refreshAll();
        }

        function playTabVideo($mediawrapper) {
            var $video = $mediawrapper.find('video');
            if ($video.length) {
                $video.get(0).play();
            }
        }

        function resetTabVideo($mediawrapper) {
            var $video = $mediawrapper.find('video');
            if ($video.length) {
                $video.get(0).pause();
                $video.get(0).currentTime = 0;
            }
        }

        // fade in current tab media (image or video)
        function toggleActiveMedia(id, callback) {
            $activeMediaOld = $activeMedia;
            $activeMedia = fullscreenImages.length > 0 ? $(fullscreenImages[id]) : $mediaWrappers.eq(playFirstVideoInFirstTab && id == 1 ? 0 : id);

            resetTabVideo($activeMedia);

            // font color change
            $element.removeClass('tabcolor-color-black');
            $element.removeClass('tabcolor-color-white');
            if (id !== 0) {
                var $openTab = $accordion.find('a[aria-expanded="true"]').closest('.panel');
                var tabColor = $openTab.attr('data-font-color');
                $element.addClass('tabcolor-' + tabColor);
            }

            if (id == 1 && $activeMediaOld.index() == 0 && playFirstVideoInFirstTab) {
                playTabVideo($activeMedia);
            } else if (id == 0 && $activeMediaOld.index() == 0 && playFirstVideoInFirstTab) {
                resetTabVideo($activeMedia);
            } else {

                if (fullscreenImages.length > 0) {
                    $activeMediaOld.css('z-index', '5');
                    $activeMedia.css('z-index', '10');
                    if (id === 0) {
                        TweenMax.to($activeMediaOld, fadeOutSpeed, {
                            alpha: 0,
                            onComplete: function (e) {
                                $activeMedia.css({ 'opacity': '', 'z-index': '' })
                                $activeMediaOld.css({ 'opacity': '', 'z-index': '' })
                            }
                        });
                    } else {
                        TweenMax.to($activeMedia, fadeOutSpeed, {
                            alpha: 1,
                            onComplete: function (e) {
                                if ($activeMediaOld.length) {
                                    $activeMediaOld.css({ 'opacity': '', 'z-index': '' })
                                }
                            }
                        });
                    }
                } else {
                    $activeMediaOld.css('z-index', '-1');
                    $accordion.find('a.accordion__panel--tab').css({ 'pointer-events': 'none', 'cursor': 'pointer' });
                    TweenMax.to($activeMediaOld, fadeOutSpeed, {
                        delay: playFirstVideoInFirstTab ? 1 : 0,
                        alpha: 0, 'margin-bottom': playFirstVideoInFirstTab ? 0 : -50,
                        onComplete: function (e) {

                            var isDefault = id === 0;

                            if (isToggleBacgroundColor && !playFirstVideoInFirstTab) {
                                updateBackgroundColor(!isDefault);
                            }

                            if (!playFirstVideoInFirstTab) {
                                var d = $activeMediaOld.index() == 0 || $activeMedia.index() == 0 ? changeBgColorSpeed : 0;
                                onHideMediaComplete($activeMedia, d, callback);
                            }

                            if (playFirstVideoInFirstTab) {
                                $(this.target).css('z-index', '');
                            }
                        }
                    });
                    if (playFirstVideoInFirstTab) {
                        onHideMediaComplete($activeMedia, 0, callback);
                    }
                }
            }
        }

        function onHideMediaComplete($media, delay, callback) {
            $media.css('z-index', '1');
            TweenMax.to($media, fadeInSpeed, {
                delay: delay,
                alpha: 1, 'margin-bottom': 0,
                onComplete: function () {
                    $accordion.find('a.accordion__panel--tab').removeAttr('style');
                    if (callback) {
                        callback($media);
                    }
                }
            });
        }

        // update module background color
        function updateBackgroundColor(toggle) {

            switch (toggle) {
                case true:
                    scope.backgroundColor = true;
                    break;
                case false:
                    scope.backgroundColor = false;
                    break;
            }

            scope.$apply();
        }

        if ($fullscreenTabImages.length) {
            loadFullscreenImages();
        }
    }

    return {
        restrict: 'E',
        link: link,
        controller: 'beo-image-accordion-ctrl'
    }
}]);
app.directive('beoVideoAccordion', ['$interval', '$timeout', function ($interval, $timeout) {
    function link(scope, element, attr) {
        
    }

    return {
        restrict: 'E',
        link: link,
        controller: 'beo-image-accordion-ctrl'
    }
}]);
app

    .directive('beoBl50PrecisionPower', [
        '$rootScope',
        function ($rootScope) {

            var setWaypoint = function ($element, offset) {

                var waypoint = new Waypoint({
                    element: $element,
                    handler: function (direction) {

                        switch (direction) {

                            case 'down':

                                TweenMax.to($element.find('.prcpwr__images_covered'), 1, { opacity: 0 }, 0.4);
                                TweenMax.to($element.find('.prcpwr__images_exposed'), 1, { opacity: 1 }, 0.4);

                                TweenMax.staggerTo($element.find('.beo-bl50-precision-power-image-annotation'), 1, { opacity: 1 }, 0.25);

                                break;

                            case 'up':

                                TweenMax.to($element.find('.prcpwr__images_exposed'), 1, { opacity: 0 }, 0.25);
                                TweenMax.to($element.find('.prcpwr__images_covered'), 1, { opacity: 1 }, 0.25);

                                TweenMax.staggerTo($element.find('.beo-bl50-precision-power-image-annotation'), 1, { opacity: 0 }, 0.1);

                                break;
                        }
                    },
                    offset: offset
                });
            };
            


            function link($scope, $element, $attrs) {

                if ($rootScope.isDesktop) {
                    setWaypoint($element, 35 + '%');
                }
                else {
                    var elementFront = $element.find('.prcpwr__images_front');
                    setWaypoint(elementFront, 40 + '%');
                    var elementBack = $element.find('.prcpwr__images_back');
                    setWaypoint(elementBack, 40 + '%');
                }
            };

            return {
                restrict: 'AE',
                link: link,
                controller: 'beo-bl50-precision-power-ctrl',
                controllerAs: '$ctrl'
            };
        }
    ])

    .directive('beoBl50PrecisionPowerImageAnnotation', [
        '$parse',
        function ($parse) {

            function link($scope, $element, $attrs) {

                $scope.$imageAnnotationTargetDiameter = 15;
                $scope.$imageInternalpPadding = 35;

                var updateLayout = function () {

                    var horizontalOffsetFn = $parse($attrs.horizontalOffset);
                    var horizontalOffset = horizontalOffsetFn($scope.$parent);

                    $scope.$direction = $attrs.anchor === 'left' ? 'row-reverse' : 'row';

                    switch ($attrs.anchor) {
                        case 'left':
                            $scope.$leftOffset = horizontalOffset;
                            $scope.$rightOffset = null;
                            break;
                        case 'right':
                            $scope.$leftOffset = null;
                            $scope.$rightOffset = horizontalOffset;
                            break;
                    }
                }

                $attrs.$observe('label', function (value) {
                    $scope.$label = decodeURIComponent(value);
                });

                $attrs.$observe('connectionWidth', function (value) {
                    $scope.$connectionWidth = parseFloat(value);
                });

                $attrs.$observe('verticalOffset', function (value) {
                    $scope.$verticalOffset = parseFloat(value);
                });

                $scope.$watch(function () {
                    return [$attrs.anchor, $attrs.horizontalOffset];
                }, updateLayout, true)
            }

            var template = '' +
                '<div class="beo-bl50-precision-power-image-annotation" ng-style="{ top: $verticalOffset, right: $rightOffset, left: $leftOffset, flexDirection: $direction }">' +
                '<span class="beo-bl50-precision-power-image-annotation--label">{{ $label }}</span>' +
                '<div class="beo-bl50-precision-power-image-annotation--connection" col ng-style="{ width: $connectionWidth }"></div>' +
                '<div class="beo-bl50-precision-power-image-annotation--target"></div>' +
                '</div>';

            return {
                restrict: 'E',
                replace: true,
                scope: {
                    label: '@label',
                    anchor: '@anchor',
                    connectionWidth: '=connectionWidth',
                    verticalOffset: '=verticalOffset',
                    horizontalOffset: '=horizontalOffset'
                },
                link: link,
                template: template
            }
        }
    ]);
app.directive(
    'beoStories',
    [
        '$window', '$rootScope',
        function ($window, $rootScope) {

            var context = $($window);

            var link = function ($scope, $element, $attrs) {

                if ($rootScope.isPhone) {
                    return;
                }

                //var storyItems;

                //var scrollListener = function ($event) {

                //    var contextHeight = context.height();
                //    var contextScrollTop = context.scrollTop();
                //    var elementOffset = $element.offset();
                //    var elementOffsetTop = elementOffset.top;
                //    var elementScrollTop = elementOffsetTop - contextScrollTop;
                //    var elementHeight = $element.height();

                //    var remainingPercent = (100 / (contextHeight / elementScrollTop));
                //    remainingPercent *= 4;
                //    remainingPercent -= 100;

                //    if (remainingPercent <= 0) {
                //        remainingPercent = 0;
                //    }
                //    else if (remainingPercent >= 100) {
                //        remainingPercent = 100;
                //    }

                //    remainingPercent = remainingPercent / 2;

                //    storyItems.each(function ($index, storyItem) {
                //        var $storyItem = $(storyItem);
                //        var storyItemHeight = $storyItem.height();
                //        var storyItemScrollSpeed = $storyItem.attr('data-scroll-speed');
                //        var storyItemFactor = storyItemHeight * storyItemScrollSpeed;
                //        var storyItemOffset = storyItemFactor * (remainingPercent / 100);
                //        storyItemOffset = parseInt(storyItemOffset);

                //        storyItemOffset = storyItemOffset === 0 ? 1 : storyItemOffset;

                //        $storyItem.css({
                //            'margin-top': storyItemOffset,
                //            opacity: ( 100 - remainingPercent ) / 100
                //        });
                //    });
                //};


                //var waypoint = new Waypoint({
                //    element: $element,
                //    handler: function (direction) {
                //        storyItems = $element.find('.beostories__story-item');
                //        switch (direction) {
                //            case 'down':
                //                context.on('scroll', scrollListener)
                //                break;
                //            case 'up':
                //                context.off('scroll', scrollListener)
                //                break;
                //        }
                //    },
                //    offset: '100%'
                //});


                var waypoint = new Waypoint({
                    element: $element,
                    handler: function (direction) {
                        switch (direction) {

                            case 'down':

                                var storyItems = $element.find('.beostories__story-item');
                                storyItems.each(function ($index, storyItem) {

                                    var storyItemSpeed = $index;
                                    if ($index == 0) {
                                        storyItemSpeed = 1.5;
                                    }

                                    storyItemSpeed++;

                                    TweenMax.to(storyItem, storyItemSpeed,
                                        {
                                            'margin-top': 0,
                                            'margin-bottom': 0
                                        }
                                    );
                                });

                                break;

                            case 'up':

                                var storyItems = $element.find('.beostories__story-item');
                                storyItems.each(function ($index, storyItem) {

                                    var storyItemSpeed = $index;
                                    if ($index == 0) {
                                        storyItemSpeed = 1.5;
                                    }

                                    storyItemSpeed++;

                                    TweenMax.to(storyItem, storyItemSpeed,
                                        {
                                            'margin-top': 600,
                                            'margin-bottom': -600
                                        }
                                    );
                                });

                                break;
                        }
                    },
                    offset: '50%'
                });
            };

            return {
                restrict: 'A',
                link: link
            };
        }
    ]
);
app.directive('beoEclipseInfiniteDepth', ['$rootScope', function ($rootScope) {
    function link(scope, element, attr) {
        // text fade in
        var scrollItems = $('.scrllvid__content');
        var videoMetaData = { height: 0, width: 0, ratio: 0, duration: 0 };
        var imageBackgroundMetaData = { height: 1920, width: 1080, ratio: (1920 / 1080) };
        var imageTvMobileMetaData = { height: 0, width: 0, ratio: 0 };
        var videoWaypoint1;
        var videoWaypoint2;
        var videoWaypoint3;
        var videoWaypoint4;
        var video = document.getElementById('scrllvid_tv_video');
        var $tvVideo = $('#scrllvid_tv_video');
        var $tvImage = $('#scrllvid_tv_image');
        var $image = $('#scrllvid_image');
        var $scrollSteps;
        var $outerWrapper = $('.scrllvid__video');
        var $contentWrp = $('.scrllvid__video--wrp-inner');
        var $copyWrp = $('.scrllvid__copy--wrp');
        var videoLoaderInterval;
        var scrollStep = 0;
        var scrollIndicatorsTotal = $rootScope.isPhone ? 50 : 100;
        var sectionsTotal = 4;
        var resizeTimer;

        function init() {
            buildIndicators();

            if ($rootScope.isPhone) {
                buildPhone();
            } else {
                buildDesktop();
            }

            onResizeTimer();
            $(window).on('resize', onWindowResize);
        }

        function buildDesktop() {
            loadTvVideo();

            loadBgImg();
        }

        function buildPhone() {
            loadTvImage();

            loadBgImg();
        }

        // for mobile
        function loadTvImage() {
            var tmpbgimg = new Image();

            //// image on metadata loaded    
            $(tmpbgimg).on('load', function () {
                imageTvMobileMetaData.height = this.height;
                imageTvMobileMetaData.width = this.width;
                imageTvMobileMetaData.ratio = this.height / this.width;

                onResizeTimer();
            });

            tmpbgimg.src = $('#scrllvid_tv_image').attr('src');
        }

        // for desktop
        function loadTvVideo() {
            videoLoaderInterval = setInterval(function () {
                if (video.readyState == 4) {
                    clearInterval(videoLoaderInterval);
                    videoLoaderInterval = null;

                    videoMetaData.height = video.videoHeight;
                    videoMetaData.width = video.videoWidth;
                    videoMetaData.ratio = video.videoHeight / video.videoWidth;
                    videoMetaData.duration = video.duration;

                    onResizeTimer();
                }
            }, 50);
        }

        function loadBgImg() {
            var tmpbgimg = new Image();

            //// image on metadata loaded    
            $(tmpbgimg).on('load', function () {
                imageBackgroundMetaData.height = this.height;
                imageBackgroundMetaData.width = this.width;
                imageBackgroundMetaData.ratio = this.height / this.width;

                onResizeTimer();
            });

            tmpbgimg.src = $('#scrllvid_image').attr('src');
        }

        function buildIndicators() {
            for (var i = 0; i < scrollIndicatorsTotal; i++) {
                $('.scrllvid__scrollindicators').append('<div class="scrllvid__scrollindicatorstep"></div>');
            }

            $scrollSteps = $('.scrllvid__scrollindicatorstep');

            $scrollSteps.eq(0).addClass('scrllvid__scrollindicatorstep--active');
        }

        // scroll
        // scrollhandlers
        function onWindowScroll(e) {
            onWindowScrollSteps();
        }

        function onWindowScrollFilter(e) {
            var p = 100 - getSectionScrollPerc(1);
            scrollItems.eq(0).css('height', p + '%');
        }

        function resetScrollFilter(h) {

            var v = h != undefined ? h : '50%';

            setTimeout(function () {
                scrollItems.eq(0).css('height', v);
            }, 100);
        }

        function onWindowScrollOutCopy1(e) {
            var p = 100 - getSectionScrollPerc(1, .75);
            p = p - 50;
            scrollItems.eq(1).css('top', p + '%');
        }

        function onWindowScrollZoomVideo(e) {
            var scrollPerc = (Math.round(getSectionScrollPerc(3, .90, false)) / 100);
            var scrollPerc2 = (Math.round(getSectionScrollPerc(3, .75, false)) / 100);
            var minZoom = $rootScope.isPhone ? .18 : .25;
            var zoomLevel = (1 - (((scrollPerc * (1 - minZoom)) + minZoom))) + minZoom;
            var contentFadeVal = 1 - scrollPerc2;

            $copyWrp.css('opacity', contentFadeVal);

            $('.scrllvid__video--wrp-inner').css('transform', 'scale(' + zoomLevel + ')');
        }

        function resetZoomLevel() {
            $('.scrllvid__video--wrp-inner').css('transform', 'scale(1)');
        }

        function getSectionScrollPerc(id, bottomOffset, hittop) {
            var offset = bottomOffset || 1;
            var hitTop = hittop ? $(window).height() : 0;
            var scrollStart = ($outerWrapper.height() - $(window).height()) / sectionsTotal;
            var scrollLength = (scrollStart - hitTop) * offset;
            var scrollElemTopY = $outerWrapper.offset().top + (scrollStart * id);
            var localScrollY = $(window).scrollTop() - scrollElemTopY;
            var scrollPerc = localScrollY / scrollLength;

            scrollPerc = scrollPerc < 1 ? (scrollPerc > 0 ? scrollPerc : 0) : 1;

            return scrollPerc * 100;
        }

        function getDocumentScrollPerc() {
            return $(window).scrollTop() / ($(document).height() - $(window).height());
        }

        function getElementScrollPerc($element, r) {
            var round = r ? r : -1;
            var s = $(window).scrollTop() - $element.offset().top;
            var height = $element.height() - $(window).height();
            var perc = s / height
            return round == -1 ? perc : Math.round((perc) * round) / round;
        }

        function onWindowResize(e) {
            clearResizeTimer();

            centerContent();

            resizeTimer = setTimeout(onResizeTimer, 150);
        }

        function onResizeTimer() {
            Waypoint.refreshAll();

            $(window).trigger('scroll');

            clearResizeTimer();

            centerContent();
        }

        function centerContent() {
            if ($rootScope.isPhone) {
                updateTvImagePosition();
            } else {
                updateTvVideoPosition();
            }
            updateImagePosition();
        }

        function clearResizeTimer() {
            if (resizeTimer) {
                clearTimeout(resizeTimer);
                resizeTimer = null;
            }
        }

        // mobile
        function updateTvImagePosition() {
            var windowRatio = window.innerHeight / window.innerWidth;
            var t = 0;
            var l = 0;

            if (windowRatio < imageTvMobileMetaData.ratio) {
                t = (- 1 * (($tvImage.height() - $(window).height()) / 2)) + 'px';
                $tvImage.css({ width: '100vw', height: 'auto' });
                $tvImage.css({ marginTop: t, marginLeft: '0' });
            } else {
                l = (- 1 * (($tvImage.width() - $(window).width()) / 2)) + 'px';
                $tvImage.css({ width: 'auto', height: '100vh' });
                $tvImage.css({ marginTop: '0', marginLeft: l });
            }
        }

        // desktop
        function updateTvVideoPosition() {
            var windowRatio = window.innerHeight / window.innerWidth;
            var t = 0;
            var l = 0;

            if (windowRatio < videoMetaData.ratio) {
                t = (- 1 * (($tvVideo.height() - $(window).height()) / 2)) + 'px';
                $tvVideo.css({ width: '100vw', height: 'auto' });
                $tvVideo.css({ marginTop: t, marginLeft: '0' });
            } else {
                l = (- 1 * (($tvVideo.width() - $(window).width()) / 2)) + 'px';
                $tvVideo.css({ width: 'auto', height: '100vh' });
                $tvVideo.css({ marginTop: '0', marginLeft: l });
            }
        }

        function updateImagePosition() {
            var windowRatio = window.innerHeight / window.innerWidth;
            var l = 0;

            if (windowRatio < imageBackgroundMetaData.ratio) {
                $image.css({ width: '100vw', height: 'auto' });

                $image.css({ marginLeft: '0' });
            } else {
                $image.css({ width: 'auto', height: '100vh' });
                l = (- 1 * (($image.width() - $(window).width()) / 2)) + 'px';

                $image.css({ marginLeft: l });
            }
        }

        // waypoints
        // 'stick' waypoint
        // scroll direction down
        videoWaypoint1 = new Waypoint({
            element: $outerWrapper,
            handler: function (direction) {
                if (direction == 'down') {
                    updateScrollStep(0);

                    $(window).on('scroll', onWindowScroll).trigger('scroll');

                    $('.scrllvid__video--wrp').addClass('scrllvid__video--fixed');
                }
            }
        });

        // 'stick' waypoint
        // scroll direction up
        videoWaypoint2 = new Waypoint({
            element: $outerWrapper.next(),
            handler: function (direction) {
                if (direction == 'up') {
                    $(window).on('scroll', onWindowScroll).trigger('scroll');

                    $('.scrllvid__video--wrp').removeClass('scrllvid__video--fixedafter').addClass('scrllvid__video--fixed');
                }
            },
            offset: '100%'
        });

        // release waypoint
        // scroll direction down
        videoWaypoint3 = new Waypoint({
            element: $outerWrapper.next(),
            handler: function (direction) {
                if (direction == 'down') {
                    updateScrollStep($scrollSteps.length - 1);

                    $(window).off('scroll', onWindowScroll);

                    $('.scrllvid__video--wrp').addClass('scrllvid__video--fixedafter').removeClass('scrllvid__video--fixed');
                }
            },
            offset: '100%'
        });

        // release waypoint
        // scroll direction up
        videoWaypoint4 = new Waypoint({
            element: $outerWrapper,
            handler: function (direction) {
                if (direction == 'up') {
                    updateScrollStep(-1);

                    $(window).off('scroll', onWindowScroll);

                    $('.scrllvid__video--wrp').removeClass('scrllvid__video--fixed');
                }
            },
        });

        //
        // showing and hiding content on scroll
        //
        contentWaypoint1 = new Waypoint({
            element: $outerWrapper,
            handler: function (direction) {
                if (direction == 'up') {
                    scrollItems.eq(1).removeClass('scrllvid__content--show');
                }

                if (direction == 'down') {
                    scrollItems.eq(1).addClass('scrllvid__content--show');
                }
            }
        });

        contentWaypoint2 = new Waypoint({
            element: $outerWrapper,
            handler: function (direction) {
                if (direction == 'up') {
                    $(window).off('scroll', onWindowScrollFilter).trigger('scroll');

                    $(window).off('scroll', onWindowScrollOutCopy1).trigger('scroll');

                    resetScrollFilter('100%');
                }

                if (direction == 'down') {
                    $(window).on('scroll', onWindowScrollFilter).trigger('scroll');

                    $(window).on('scroll', onWindowScrollOutCopy1).trigger('scroll');
                }
            }, offset: function () {
                return - 1 * ((($outerWrapper.height() - $(window).height()) / sectionsTotal) * 1);
            }
        });

        contentWaypoint3 = new Waypoint({
            element: $outerWrapper,
            handler: function (direction) {
                if (direction == 'up') {
                    scrollItems.eq(1).addClass('scrllvid__content--show');
                    scrollItems.eq(2).removeClass('scrllvid__content--show');

                    $('.scrllvid__scrollindicators').addClass('scrllvid__scrollindicators--margani');

                    $('.scrllvid__scrollindicators').removeClass('scrllvid__scrollindicators--movetotop');

                    $(window).on('scroll', onWindowScrollFilter).trigger('scroll');

                }

                if (direction == 'down') {
                    scrollItems.eq(2).addClass('scrllvid__content--show');

                    $('.scrllvid__scrollindicators').addClass('scrllvid__scrollindicators--movetotop scrllvid__scrollindicators--margani');

                    $(window).off('scroll', onWindowScrollFilter).trigger('scroll');

                    resetScrollFilter('0');
                }
            }, offset: function () {
                return - 1 * ((($outerWrapper.height() - $(window).height()) / sectionsTotal) * 2);
            }
        });

        contentWaypoint4 = new Waypoint({
            element: $outerWrapper,
            handler: function (direction) {
                if (direction == 'up') {
                    $(window).off('scroll', onWindowScrollZoomVideo);

                    resetZoomLevel();
                }

                if (direction == 'down') {
                    $(window).on('scroll', onWindowScrollZoomVideo).trigger('scroll');

                }

            }, offset: function () {
                return - 1 * ((($outerWrapper.height() - $(window).height()) / sectionsTotal) * 3);
            }
        });

        //
        // scroll indicator
        $('.scrllvid__scrollindicators').on('webkitTransitionEnd otransitionend oTransitionEnd msTransitionEnd transitionend', onMovedToTop);

        function onMovedToTop(e) {
            if (e.originalEvent.propertyName == 'margin-top') {
                $('.scrllvid__scrollindicators').removeClass('scrllvid__scrollindicators--margani');
            }
        }

        function onWindowScrollSteps() {
            var scrolledPerc = getElementScrollPerc($outerWrapper, 100000);
            var percOfItems = Math.round($scrollSteps.length * scrolledPerc);
            var margTop = - 1 * ((scrolledPerc * $('.scrllvid__scrollindicators').height()) * .75);

            updateScrollStep(percOfItems);

            $('.scrllvid__scrollindicators').css('margin-top', margTop + 'px');
        }

        function updateScrollStep(s) {
            if (s != scrollStep) {
                scrollStep = s > $scrollSteps.length - 1 ? $scrollSteps.length - 1 : (s < 0 ? 0 : s);

                $scrollSteps.filter('.scrllvid__scrollindicatorstep--active').removeClass('scrllvid__scrollindicatorstep--active');

                $scrollSteps.eq(scrollStep).addClass('scrllvid__scrollindicatorstep--active');
            }
        }

        init();
    }
    return {
        link: link
    }
}]);
app.directive('beoSoundGoogleVoiceAssistant', ['$rootScope', function ($rootScope) {
    function link(scope, element, attr) {
        var $backgroundImage = element.find('.gva__parallaxbackgroundimage');
        var $captions = element.find('.gva__content');
        var $outerWrapper = element.find('.gva__parallax');
        var $innerWrapper = element.find('.gva__parallax--wrp');
        var $googleLogo = element.find('.gva__googleassistant')
        var $background = $('.gva__parallaxbackground');
        var $scrollSteps;
        var scrollBlocker = new ScrollBlocker({
            $element: $outerWrapper,
            onUpdate: function (i) {
                if ($(window).width() <= 768 && i >= $captions.length - 1) {
                    $googleLogo.addClass('gva__googleassistant--visible')
                } else {
                    $googleLogo.removeClass('gva__googleassistant--visible')
                }
            }
        })
        var controller = new ScrollMagic.Controller();
        var textParallax = setTextParallax();
        var imageStickyOutro = setImageStickyOutro();

        function init() {
            loadBackgroundImage();
            updateParallax()
            $(window).on('scroll', scrollBlocker.handleScroll)
            $(window).on('resize', updateParallax)
            $(window).on('wheel mousewheel touchmove touchdown', scrollBlocker.handleWheel)
        }

        function updateParallax() {
            textParallax.duration(getHeight());
            imageStickyOutro.duration(getHeight());
        }

        // get screen height polyfill

        function getHeight() {
            return $('html').hasClass('phone') ? window.innerHeight : $(window).height()
        }

        // parallax Text

        function setTextParallax() {
            function parallaxText(e) {
                var margin = $('html').hasClass('phone') ? 10 : getHeight() / 2;
                var progress = e.target.progress()
                var distance = (getHeight() - margin) * (1 - progress) + 'px';
                $captions.eq(0).css({
                    'transform': 'translate3d(0,' + distance + ',0)',
                    'display': ($('html').hasClass('phone') && progress <= 0) ? 'none' : '',
                    'transition': (progress < 1) ? 'none' : ''
                });
                $background.css({
                    'transform': 'translate3d(0,' + (getHeight() * 2) * (1 - progress) + 'px,0)',
                    'display': progress === 0 ? 'none' : ''
                })
            }

            return new ScrollMagic.Scene({ triggerElement: $outerWrapper[0], triggerHook: 1 })
                .on('update', parallaxText)
                .addTo(controller);
        }

        // parallax out

        function setImageStickyOutro() {
            function stickyOutro(e) {
                var style = {}
                style.height = e.target.progress() === 0 ? '' : (1 - e.target.progress()) * getHeight() + 'px'
                style.display = e.target.progress() < 1 ? '' : 'none'
                $background.css(style);
                $captions.css(style);
            }

            return new ScrollMagic.Scene({ triggerHook: 1, triggerElement: $outerWrapper.next()[0] })
                .on('update', stickyOutro)
                .addTo(controller);
        }


        // scrollBlocker

        function ScrollBlocker(config) {
            this.$element = config.$element;
            this.$slides = config.$element.find('.gva__slide');
            this.$indicators = buildIndicators.bind(this)();
            this.index = 0;
            this.lastTouchY = 0;
            this.touches = 0;
            this.touchDirection = 0;
            this.lastScrollY = 0;
            this.lastAnimationTime = Date.now();
            this.onUpdate = config.onUpdate

            $('html').addClass('scrollblock')

            var scope = this

            function buildIndicators() {
                for (var i = 0; i < $captions.length; i++) {
                    this.$element.find('.gva__scrollindicators').append('<div class="gva__scrollindicatorstep"></div>');
                }
                return this.$element.find('.gva__scrollindicatorstep');
            }

            this.activateSection = function (i) {
                this.$slides.removeClass('scrollblock-slide--above scrollblock-slide--active scrollblock-slide--below');
                this.$slides.eq(i - 1).addClass('scrollblock-slide--above');
                this.$slides.eq(i).addClass('scrollblock-slide--active');
                this.$slides.eq(i + 1).addClass('scrollblock-slide--below');
            }

            this.addActiveClass = function () {
                $('html').removeClass('scrollblock--out-bottom scrollblock--out-top');
                $('html').addClass('scrollblock--active');
            }

            this.addLeavingClass = function (direction) {
                var outClassName = 'scrollblock--out-' + (direction === 1 ? 'bottom' : 'top');
                $('html').addClass(outClassName);
                $('html').removeClass('scrollblock--active');
            }

            this.animateUpdates = function (direction) {
                this.updateIndex(direction);
                this.updateImagePosition();
                this.onUpdate && this.onUpdate(this.index);
                requestAnimationFrame(this.goToSetionIndex);
                this.setScrollLockTimeout()
            }

            this.goToSetionIndex = function () {
                this.goToSection(this.index)
            }


            this.goToSection = function (i) {
                this.setActiveIndex(i)
                $("html, body").scrollTop(this.$slides.eq(this.index).offset().top);
            }

            this.handleScroll = function (e) {
                var direction = $(window).scrollTop() > scope.lastScrollY ? 1 : -1
                scope.onScroll(e, direction);
                scope.lastScrollY = $(window).scrollTop()
            }

            this.handleWheel = function (e) {
                if (scope.isAnimating()) return scope.preventDefault(e)
            }

            this.isAnimating = function () {
                return Date.now() - this.lastAnimationTime < 1200
            }

            this.isInHitArea = function () {
                return $(window).scrollTop() > (this.$element.offset().top - 2) && ($(window).scrollTop() + getHeight()) < (this.$element.offset().top + this.$element.height() + 1)
            }

            this.isLeaving = function (direction) {
                var next = this.index + direction
                return next < 0 || next > (this.$slides.length - 1)
            }

            this.delayIntro = function () {
                this.addActiveClass()
                this.setScrollLockTimeout(this.delayIntro1);
            }

            this.delayIntro1 = function () {
                this.animateUpdates(1);
            }

            this.onScroll = function (e, direction) {
                if (this.isAnimating()) return this.preventDefault(e);
                if (this.isInHitArea() && this.index <= 0 && direction === 1 && !$('body').hasClass('phone')) this.delayIntro();
                if (this.isInHitArea() && !this.isLeaving(direction)) this.runActive(e, direction)
                if (this.isLeaving(direction)) this.runLeaving(direction)
            }

            this.preventDefault = function (e) {
                e.preventDefault();
                e.stopPropagation();
                $("html, body").scrollTop(scope.$slides.eq(scope.index).offset().top);
                return false
            }

            this.runActive = function (e, direction) {
                this.preventDefault(e);
                this.addActiveClass()
                if (!this.isAnimating()) this.animateUpdates(direction)
            }

            this.runLeaving = function (direction) {
                this.addLeavingClass(direction)
            }

            this.setActiveIndex = function (i) {
                this.activateSection(i)
                this.updateIndicators(i)
            }

            this.setScrollLockTimeout = function (callback) {
                if (!this.isAnimating()) {
                    this.lastAnimationTime = Date.now()
                    callback && callback()
                }
            }

            this.updateImagePosition = function () {
                var distance = $('html').hasClass('phone') ? -(getHeight() / 3.5) + 'px' : -(((this.index + 1) / this.$slides.length) * ($backgroundImage.height() - getHeight())) + 'px'
                $backgroundImage.css({
                    'transform': 'translate3d(0,' + distance + ',0)'
                });
            }

            this.updateIndicators = function (i) {
                this.$indicators.removeClass('gva__scrollindicatorstep--active')
                this.$indicators.eq(i).addClass('gva__scrollindicatorstep--active')
            }

            this.updateIndex = function (direction) {
                this.index += direction
            }

            this.setActiveIndex(0)

        }

        // for mobile

        function loadBackgroundImage() {
            var url = $backgroundImage.attr($rootScope.isPhone ? 'data-image-mobile' : 'data-image-desktop');
            var tmpbgimg = new Image();

            $(tmpbgimg).on('load', function (e) {
                $backgroundImage.css({ 'background-image': ('url(' + url + ')'), 'height': ($backgroundImage.attr('data-height') + 'vh') }).next().hide();
            });

            tmpbgimg.src = url;
        }

        init();
    }
    return {
        link: link
    }
}]);
app.directive('beoVideoImageSplit', ['$timeout', '$rootScope', function ($timeout, $rootScope) {
    function link(scope, element, attr) {
        var loopDelay = parseFloat(attr.videoLoopDelay);
        var videoJs = videojs(element.find('video').attr('id'));
        var loop;

        videoJs.ready(onVideoJsReady)

        function onVideoJsReady() {
            loop = videoJs.loop();

            videoJs.loop(false);

            videoJs.on('ended', onVideoEnded);
        }

        function onVideoEnded() {
            if (loop) {
                videoJs.pause();

                setTimeout(function () {
                    videoJs.play();
                }, (loopDelay * 1000));
            }
        }
    }

    return {
        link: link
    };
}]);
app.directive(
    'beoScrollColumns',
    [
        '$window', '$rootScope',
        function ($window, $rootScope, $timeout) {
            
            var context = $($window);

            var link = function ($scope, $element, $attrs) {
                
                var storyItems = $element.find('.beostories__story-item');
                
                if ($rootScope.isPhone) {
                    return;
                }
                var scrollListener = function ($event) {

                    var contextHeight = context.height();
                    var contextScrollTop = context.scrollTop();
                    var elementOffset = $element.offset();
                    var elementOffsetTop = elementOffset.top;
                    var elementScrollTop = elementOffsetTop - contextScrollTop;
                    var elementHeight = $element.height();


                    var remainingPercent = (100 / (contextHeight / elementScrollTop));
                    remainingPercent *= 4;
                    remainingPercent -= 100;

                    if (remainingPercent <= 0) {
                        remainingPercent = 0;
                    }
                    
                    remainingPercent = remainingPercent / 2;
                    storyItems.each(function ($index, storyItem) {
                        var $storyItem = $(storyItem);
                        var storyItemScrollSpeed = $storyItem.attr('data-scroll-speed');
                        var storyItemFactor = 50 * storyItemScrollSpeed;
                        var storyItemOffset = storyItemFactor * (remainingPercent / 100);
                        storyItemOffset = parseInt(storyItemOffset);

                        storyItemOffset = storyItemOffset <= 0 ? 0 : storyItemOffset;
                        $storyItem.css({
                            'margin-top': storyItemOffset
                        });
                    });

                };

                var lastScrollTop = 0;
                $(window).scroll(function () {
                    scrollListener();
                });
            };

            return {
                restrict: 'A',
                link: link
            };
        }
    ]
);
app.directive(
    'beoScrollColumnsReverse',
    [
        '$window', '$rootScope',
        function ($window, $rootScope, $timeout) {
            
            var context = $($window);

            var link = function ($scope, $element, $attrs) {

                var column = $element.find('.cmpgnslr--scroll-reverse');

                if ($rootScope.isPhone) {
                    return;
                }

                var scrollListener = function ($event) {

                    var contextHeight = context.height();
                    var columnHeight = $element.height();
                    var windowScrollPos = window.pageYOffset;
                    var columnFromTop = $(column).offset().top;
                    var startAnimatePos = columnFromTop - columnHeight;

                    var columnPxFromTop = startAnimatePos - windowScrollPos;

                    var remainingPercent = (100 / (contextHeight / columnPxFromTop));

                    if (remainingPercent >= 0) {
                        columnPxFromTop = 0;
                    }

                    column.each(function ($index, storyItem) {

                        var $column = $(storyItem);
                        var columnScrollSpeed = $column.attr('data-scroll-speed');
                        var columnOffset = columnScrollSpeed * remainingPercent;
                        columnOffset = parseInt(columnOffset);
                        

                        columnOffset = columnOffset > 0 ? 0 : columnOffset;
                        columnOffset = columnOffset < -100 ? -100 : columnOffset;

                        $column.css({
                            "-moz-transform": "translatey(" + columnOffset + "px)",
                            "-o-transform": "translatey(" + columnOffset + "px)",
                            "-webkit-transform": "translatey(" + columnOffset + "px)",
                            "transform": "translatey(" + columnOffset + "px)"
                        });
                    });
                };

                var waypoint = new Waypoint({
                    element: $element,
                    handler: function (direction) {
                        switch (direction) {
                            case 'down':
                                context.on('scroll', scrollListener);
                                break;
                            case 'up':
                                context.off('scroll', scrollListener);
                                break;
                        }
                    },
                    offset: '100%'
                });

            };

            return {
                restrict: 'A',
                link: link
            };
        }
    ]
);
app.directive('beoColorPicker', ['$rootScope', function ($rootScope, beoSearchService) {

    var link = function ($scope, $element, $attrs) {

        $scope.dropdownOpen = false;
        $scope.selectedColor = $element.find('.colorpicker__slider--container').prop("id");

        $scope.setColor = function (event) {
            $scope.selectedColor = event.currentTarget.attributes['data-id'].value;
            event.stopPropagation();
            event.preventDefault();
        };

        $scope.$watch('dropdownOpen', function (newValue) {
            if (newValue) {
                $scope.dropdownHeight = $element.find('.colorpicker__infoModule--feature--dropdown ul').height();
            } else {
                $scope.dropdownHeight = 0;
            }
        });

        function setSliderHeight() {
            var sliderHeight = $element.find('.colorpicker__slider--container').height();
            $element.find('.colorpicker__slider--container').parent().css({ "height": sliderHeight, "position": "relative" });
        }

        $(window).load(function () {
            $('.flexslider').flexslider({
                animation: "fade",
                slideshow: false,
                directionNav: false,
                animationSpeed: 600,
                useCCS: true,
                start: function (slider) {
                    setActiveAttr(slider);
                },
                before: function (slider) {
                    slider.slides.eq(slider.currentSlide).delay(0);
                    slider.slides.eq(slider.animatingTo).delay(600);

                },
                after: function (slider) {
                    setActiveAttr(slider);
                }
            });

            $('.colorpicker__slider--controles .slider-prev').on('click', function () {
                $(this).closest(".flexslider").flexslider('prev');
                return false;
            });

            $('.colorpicker__slider--controles .slider-next').on('click', function () {
                $(this).closest(".flexslider").flexslider('next');
                return false;
            });

            function setActiveAttr(slider) {
                var $activeSlider = $('.colorpicker__slider--container.active');
                var $allSlides = $('.colorpicker__slider--container');
                $allSlides.find('li').removeAttr('aria-current');
                $activeSlider.find('.flex-active-slide').attr('aria-current', 'true');
                $activeSlider.find('li').not('.flex-active-slide').removeAttr('aria-current');
            }

            setSliderHeight();

            $(window).resize(function () {
                setSliderHeight();
            });
        });
    };

    return {
        restrict: 'A',
        replace: true,
        link: link
    };

}]);
app.directive('beoAlignHeight', [function () {
    function link($scope, $element, $attrs) {

        var target = $element.find('.beostories__story-item-image');

        function alignHeight() {

            var lastHeight = 0;

            target.each(function ($index, item) {
                var $curItem = $(item);
                var curItemHeight = $curItem.height();

                if (curItemHeight < lastHeight) {
                    $curItem.css({
                        "height": lastHeight
                    });
                }
                else if (curItemHeight > lastHeight) {
                    lastHeight = curItemHeight;
                    $curItem.css({
                        "height": "auto"
                    });
                }
            });
        }

        $(window).on('resize', function (e) {
            alignHeight();
        });

    }
    return {
        restrict: 'A',
        link: link
    }
}]);
app.directive('beoProductCategorySlider', ['$rootScope', function ($rootScope) {
    var link = function (scope, element, attrs) {
        var totalProductsInCategory = parseInt(attrs.productsTotal);
        var $productslider = element.find('.cardslider--slider');
        var $productCols = element.find('.cardslider__card');
        //var dd;
        var isDragdealerEnabled = false;
        var colOffset = attrs.colOffset ? (parseFloat(attrs.colOffset, 10) / 100) : 0;
        var winWidth = 100 - ((colOffset * 2) * 100);
        var paddingRight = attrs.paddingRight ? attrs.paddingRight : 0;
        var smallImages = [];
        var imageMaxHeight = 0;
        var imageLoadCount = 0;
        var $images = element.find('.beostories__storyimg img');
        
        $(window).on('resize', onWindowResize).trigger('resize');

        setSliderwidth();

        //dd = new Dragdealer(attrs.id, { disabled: !isDragdealerEnabled });

        // IE11 fallback
        // If <picture> is not supported
        if (!Modernizr.picture) {
            $images.eq(0).one("load", function () {
                imageMaxHeight = Math.max($(this).height(), imageMaxHeight);
                
                element.find('.beostories__storyimg').each(function (i) {
                    if (i > 0) {
                        $(this).addClass('adjust-height');
                    }
                });

                $(window).trigger('resize');
                
            }).each(function () {
                if (this.complete) $(this).load();
            });
        }

        element.addClass('cardslider--show');


        function onWindowResize(e) {
            setSliderwidth();
        }

        function setSliderwidth() {

            if (element.css('position') == 'relative') {
                $productslider.css({
                    'width': ((100 * totalProductsInCategory) + '%')
                });
                element.css({ 'padding-right': paddingRight });

                $productCols.css('width', (100 / totalProductsInCategory) + '%');

                //if (!isDragdealerEnabled) {
                //    toggleDragdealer(true);

                //    isDragdealerEnabled = true;
                //}

                //if (dd) {
                    //dd.reflow();
                //}

                // IE11 fallback
                // If <picture> is not supported
                if (!Modernizr.picture) {
                    var h = 0;
                    element.find('.beostories__storyimg.adjust-height').each(function (i) {
                        $(this).height($images.eq(0).height());
                    });
                    
                }
            } else {

                if (isDragdealerEnabled) {
                    toggleDragdealer(false);

                    isDragdealerEnabled = false;
                }

                $productCols.css('width', '');
                $productslider.css({
                    'width': ''
                });
                element.css({ 'padding-right': '' });

                if (!Modernizr.picture) {
                    element.find('.beostories__storyimg.adjust-height').each(function (i) {
                        $(this).height('');
                    });
                }
            }
        }

        function toggleDragdealer(enable) {
            if (dd) {
                switch (enable) {
                    case true:
                        dd.enable();
                        break;

                    case false:
                        dd.disable();
                        dd.setValue(0, 0);
                        break;
                }
            }
        }

        function onAltmenuOpen(e) {
            if (isElementInAltMenu) {
                setSliderwidth();
            }
        }

        function onAltmenuClosed(e) {
            if (isElementInAltMenu) {
                //dd.setValue(0, 0);
            }
        }


    };

    return {
        restrict: 'EA',
        link: link
    };
}]);

app.directive('beoShowOnImagesLoaded', ['$rootScope', function ($rootScope) {
    var link = function (scope, element, attrs) {
        var c = 0;

        element.find('img').one('load', function () {
            c++;
            element.addClass('showonimagesloaded--show');
        }).each(function () {
            if (this.complete) $(this).load();
        });
    };

    return {
        restrict: 'EA',
        link: link
    };
}]);

app.directive('beoShowCampaignslider', ['$rootScope', function ($rootScope) {
    var link = function (scope, element, attrs) {
        var $firstSliderImage = element.find('.flexslider li:first-child img');
        var $secImgs = element.find('.cmpgnslr__seccmpgn img');
        var c = 0;

        $firstSliderImage.one('load', function () {
            onImageLoaded();
        }).each(function () {
            if (this.complete) $(this).load();
        });

        $secImgs.one('load', function () {
            onImageLoaded();
        }).each(function () {
            if (this.complete) $(this).load();
        });

        function onImageLoaded(e) {
            //c++;
            //if (c == ($secImgs.length + $firstSliderImage.length)) {
            //    element.addClass('beoshowcampaignslider--show');
            //}

            if (c == 0) {
                element.addClass('beoshowcampaignslider--show');
            }
            c++;
        }
    };

    return {
        restrict: 'A',
        link: link
    };
}]);

// how to use:
// wrap image/picture in div and apply "beo-background-image-cover" directive to wrapping div.
// 
// example:
// 
// <div beo-background-image-cover>
//     <picture>
//         <source srcset="">
//         <img src="">
//     </picture>
// </div>
//
// or:
// 
// <div beo-background-image-cover>
//     <img src="">
// </div>
//
app.directive('beoBackgroundImageCover', ['UtilsService', function (UtilsService) {
    var link = function (scope, element, attrs) {

        var $img,
            $video,
            $media,
            mediatype,
            height, width, dimensions, ratio;

        function onImageLoad() {
            $media = $img;

            // image loaded
            width = parseInt($img.width());
            height = parseInt($img.height());
            ratio = width / height;

            $(window).on('resize', onResize).trigger('resize');
        }

        function onVideoLoaded(e, w, h) {
            $media = $video;

            width = parseInt(w);
            height = parseInt(h);
            ratio = width / height;

            $(window).on('resize', onResize).trigger('resize');
        }

        function onResize(e) {

            dimensions = UtilsService.getFullscreenImageDimensions({ image: $media, context: element, orgwidth: width, orgheight: height });
            $media.css({
                'width': (dimensions.width + 'px'),
                'height': (dimensions.height + 'px'),
                'margin-left': (dimensions.marginLeft + 'px'),
                'margin-top': (dimensions.marginTop + 'px')
            });

            $media.addClass("resized");
        }

        setTimeout(function () {
            $img = element.find('img');
            $video = element.find('video');
            $media;
            mediatype = $img.length ? 'image' : 'video';
            height, width, dimensions, ratio;

            if (mediatype === 'image') {
                $img.on('load', onImageLoad).each(function () {
                    if (this.complete) $(this).load();
                });
            } else {
                $video[0].load();
                $video[0].addEventListener('loadedmetadata', function (e) {
                    onVideoLoaded(e, this.videoWidth, this.videoHeight);
                }, false);
            }
        }, 100);
    };

    return {
        link: link
    };
}]);

app.directive('beoTriggerImageOnload', ['$rootScope', function ($rootScope) {
    var link = function (scope, element, attrs) {
        var $image = $(element).prop('nodeName').toLowerCase() === 'picture' ? $(element).find('img') : $(element);
        
        $image.one('load', onLoad);

        if ($image.get(0).complete) {
            $image.load();
        }

        function onLoad(e) {
            $rootScope.$broadcast('imageLoaded', element.context);
        }
    };

    return {
        restrict: 'EA',
        link: link
    };
}]);

app.directive('beoNewsletterSignupFooterModal', ['$rootScope', 'beoNewsletterSignupService', 'beoNewsletterUnsubscribeService', '$timeout', function ($rootScope, beoNewsletterSignupService, beoNewsletterUnsubscribeService, $timeout) {
    var link = function (scope, element, attrs) {

        //var $formWrp = element.find('#subscribe_newsletterForm_modal');
        var $modalBody = element.find('.modal-body');
        var colorsObj = attrs.modalColors != undefined ? eval('(' + attrs.modalColors + ')') : null;
        var c = 0;
        var breakLoop = false;

        scope.newsletterModalBackgroundColor = colorsObj.backgroundColorStep1;

        $rootScope.$on('newsletter_signup_submit', onNewsletterSignupSubmit);

        if (element.hasClass("confirmationView")) {
            scope.formHide = true;
            $('#footer_newsletter_modal').modal('show');

            $('#footer_newsletter_modal').on('show.bs.modal', onModalShow);
            $('#footer_newsletter_modal').on('hidden.bs.modal', onModalHidden);

            scope.newsletterModalBackgroundColor = colorsObj.backgroundColorStep3;

            if (element.hasClass("confirmationViewFailed")) {
                scope.showSignupError = true;
            }
            else
                scope.showSignupConfirmed = true;
        }

        Object.keys(colorsObj).forEach(function (key) {
            if (!breakLoop && colorsObj[key].length) {
                element.addClass('modal__footernewslettermodal--bgtransition');
                breakLoop = true;
            }
            c++;
        });

        $rootScope.$watch('footer.email', function (newval, oldval) {
            scope.user.email = newval;
        });
        
        scope.displayForm = function (element) {
            $('#collapseOpenButton').fadeOut('fast', myTimelineRun());
            function myTimelineRun() {
                var tl = new TimelineMax({ delay: 0.2 }).timeScale(0.5);
                tl.to($('.row-one'), 1, { autoAlpha: 1, display: 'block' });
                tl.to($('.row-two'), 1, { autoAlpha: 1, display: 'block' }, "-=0.7");
                tl.to($('.row-three'), 1, { autoAlpha: 1, display: 'block' }, "-=0.7");
                tl.to($('#registerButton'), 1, { autoAlpha: 1, display: 'block' }, "-=0.7");
            }
        }

        function onNewsletterSignupSubmit(e) {
            scope.newsletterModalBackgroundColor = colorsObj.backgroundColorStep2;
        }

        function onModalShow(e) {
            console.log('show');
            $('html').css('overflow', 'hidden');
        }

        function onModalHidden(e) {
            console.log('hidden');
            $('html').css('overflow', '');
        }
    };

    return {
        restrict: 'EA',
        link: link
    };
}]);
app.directive('beoPermissionSignup', ['$rootScope', function ($rootScope) {
    var link = function (scope, element, attrs) {

        var $imgWrp = element.find('.permsignup__packshot--desktop');
        var $img = $imgWrp.find('img');
        var modulePadding = element.outerHeight() - element.height();

        if ($imgWrp.length) {
            $(window).on('resize', onWindowResize).trigger('resize');
        }

        function onWindowResize(e) {
            var marginBottom, modulePadding;

            modulePadding = element.outerHeight() - element.height();
            marginBottom = ($img.height() + parseInt($imgWrp.css('margin-top'), 10)) - element.height() - (modulePadding / 2)
            
            if ($imgWrp.is(':visible') && marginBottom > 0) {
                element.css('margin-bottom', marginBottom + 'px');
            } else {
                element.css('margin-bottom', '');
            }

            if ($imgWrp.hasClass('permsignup--faded')) {
                $imgWrp.removeClass('permsignup--faded');
            }
        }
    };

    return {
        restrict: 'EA',
        link: link
    };
}]);

app.directive('beoPlayAccordion', ['$rootScope', '$compile', 'UtilsService', 'beoPlayAccordionService', function ($rootScope, $compile, UtilsService, beoPlayAccordionService) {
    var link = function (scope, element, attrs) {
        var $defaultBackgroundImages = element.find('.playacc__backgroundimg');
        var $tabs = element.find('.playacc__tab');
        var $tablabels = element.find('.playacc__tablabel');
        var $tablabelsOpen = element.find('.playacc__tablabel-open');
        var $tablabelsOpenIconMobile = element.find('.playacc__tabicon--open-mobile');
        var $tabImagesUl = element.find('.playacc__tabimageul');
        var $tabLiVideos = element.find('.playacc__tabimageli--video');
        var $tabImageLi;
        var openTabHeight = 70; //%
        var openTabWidth = { sm: 70, md: 80, lg: 80 }; //%
        var mq = element.find('.playacc__mq');

        scope.playaccOpened = false;
        scope.playaccOpen = false;
        scope.playaccOpening = false;
        scope.playaccClosing = false;
        scope.playaccClosed = true;
        scope.playaccHideDefaultBgImg = false;
        scope.playaccTabColorWhite = false;
        scope.playaccTabColorBlack = false;

        $tabs.on('webkitTransitionEnd otransitionend oTransitionEnd msTransitionEnd transitionend', onTabsTransitionEnd);

        $rootScope.$on('playacc_scroll_complete', onPlayaccScrollComplete);

        function init() {
            var imgSrcArrTmp = [];

            $tabs.each(function () {
                imgSrcArrTmp.push({
                    'mobile': UtilsService.hasAttr($(this), 'data-mobile-image') ? $(this).attr('data-mobile-image') : '',
                    'desktop': UtilsService.hasAttr($(this), 'data-desktop-image') ? $(this).attr('data-desktop-image') : '',
                    'video': UtilsService.hasAttr($(this), 'data-video-url') ? $(this).attr('data-video-url') : '',
                    'videoPoster': UtilsService.hasAttr($(this), 'data-video-poster-url') ? $(this).attr('data-video-poster-url') : ''
                });
            });
            preloadImages(imgSrcArrTmp);

            //
            //
            $tabLiVideos.each(function () {
                beoPlayAccordionService.resizeVideo($(this), $(this).find('video'));
            });
        }

        scope.onTabClick = function ($event) {
            var $tab = $($event.currentTarget).closest('.playacc__tab');

            // if clicked tab is already open or accordion is closing return false
            if ($tab.hasClass('playacc__tab--active') || scope.playaccClosing) {
                return false;
            }

            // fix tab labels width to avoid wrapping
            if (scope.playaccOpened) {
                // opening new tab when accordion is allready open
                $tablabels.width($tabs.filter('.playacc__tab--active').find('.playacc__tablabel').width());
            } else {
                // set a fixed width on tab labels to avoid textfield widths scaling when tabs are opening
                $tablabels.width($tablabels.width());
            }

            // removes 'active' class from previously active tab
            $tabs.filter('.playacc__tab--active').removeClass('playacc__tab--active');

            // hides tab-label-opned label
            $tabs.filter('.playacc__tab--opened').removeClass('playacc__tab--opened');

            // set current tab as active
            $tab.addClass('playacc__tab--active');

            setCollapsedTabsWidth();

            $(window).on('resize', onWindowResize);

            scope.playaccTabColorBlack = $tab.hasClass('color-black') ? true : false;
            scope.playaccTabColorWhite = $tab.hasClass('color-white') ? true : false;

            // show current tab background image
            updateBackgroundImage($tab.index());

            scope.playaccOpen = true;

            scope.playaccOpening = true;

            scope.playaccClosed = false;

        };

        scope.onCloseClick = function ($event) {
            var $tab = $($event.currentTarget).closest('.playacc__tab');

            if (scope.playaccOpen && $tab.hasClass('playacc__tab--active')) {
                // reset all classes
                $tabs.filter('.playacc__tab--active').removeClass('playacc__tab--active');

                $tabs.css({ 'height': ((100 / $tabs.length) + '%'), 'width': ((100 / $tabs.length) + '%') });

                $tabs.filter('.playacc__tab--opened').removeClass('playacc__tab--opened');

                // fade out tab image
                $tabImageLi.filter('.playacc__tabimageli--show').addClass('playacc__tabimageli--hide');

                element.find('.playacc__tabicon--open-mobile').css('top', '50%');

                scope.playaccTabColorBlack = false;
                scope.playaccTabColorWhite = false;

                scope.playaccHideDefaultBgImg = false;

                scope.playaccOpened = false;

                scope.playaccOpen = false;

                scope.playaccClosing = true;
            }
        };

        function onPlayaccScrollComplete(e, d) {
            $(d).find('.playacc__tabbtn').trigger('click');
        }

        function setCollapsedTabsWidth() {
            $tabs.filter(':not(.playacc__tab--active)').css({ 'height': getCalculatedOpenTabHeightStr(), 'width': getCalculatedOpenTabWidthStr() });
        }

        function getTabOpenWidth() {
            return openTabWidth[mq.filter(':visible').attr('data-mq')];
        }

        function getCalculatedOpenTabWidthStr() {
            return (100 - getTabOpenWidth()) / ($tabs.length - 1) + '%';
        }

        function getCalculatedOpenTabHeightStr() {
            return ((100 - openTabHeight) / ($tabs.length - 1)) + '%';
        }

        function onWindowResize(e) {
            setCollapsedTabsWidth();
        }

        // on accordion tabs
        function onTabsTransitionEnd(e) {
            if (scope.playaccOpening) {
                // on accordion opened
                if (e.originalEvent.propertyName === 'flex-grow' || e.originalEvent.propertyName === 'width' || e.originalEvent.propertyName === 'height') {
                    // add 'opened' class to current activetab
                    $tabs.filter('.playacc__tab--active').addClass('playacc__tab--opened');

                    // remove tab content fixed widths
                    $tablabels.prop('style', false);

                    scope.playaccOpened = true;

                    scope.playaccOpening = false;

                    scope.$apply();
                }
            }

            if (scope.playaccClosing) {
                // on accordion closed
                if (e.originalEvent.propertyName === 'flex-grow' || e.originalEvent.propertyName === 'width' || e.originalEvent.propertyName === 'height') {
                    // remove tab content fixed widths
                    $tablabels.prop('style', false);

                    $(window).off('resize', onWindowResize);

                    $tablabelsOpen.prop('style', false);
                    $tablabelsOpenIconMobile.prop('style', false);

                    scope.playaccClosing = false;

                    scope.playaccClosed = true;

                    scope.$apply();
                }
            }
        }

        // update accordion tab images
        function updateBackgroundImage(index) {
            var $previousBackground = $tabImageLi.filter('.playacc__tabimageli--show');
            var $newBackground = $tabImageLi.eq(index);
            var $video = $newBackground.find('video');

            // moved previous active image behind current image
            if ($previousBackground.length) {
                $previousBackground.addClass('playacc__tabimageli--previous');
            }

            $newBackground.addClass('playacc__tabimageli--show');

            $video = $newBackground.find('video');
        }

        // on image fade-in/transition complete
        function onImageTransitionEnd(e) {
            if ($tabImageLi.filter('.playacc__tabimageli--previous').length) {
                $tabImageLi.filter('.playacc__tabimageli--previous').removeClass('playacc__tabimageli--show playacc__tabimageli--previous');
                return false;
            }

            if ($tabImageLi.filter('.playacc__tabimageli--hide').length) {
                $tabImageLi.removeClass('playacc__tabimageli--show playacc__tabimageli--previous playacc__tabimageli--hide');
                return false;
            }

            if (!$tabImageLi.filter('.playacc__tabimageli--previous').length) {
                scope.playaccHideDefaultBgImg = true;
                scope.$apply();
                return false;
            }
        }

        // preload accordion tab background images and add to DOM
        function preloadImages(arr) {
            for (var i = 0; i < arr.length; i++) {
                var $tmpli = $tabImagesUl.find('.playacc__tabimageli').eq(i);

                if (arr[i].desktop.length) {
                    $tmpli.find('.playacc__tabimage--desktop').css('background-image', 'url(' + arr[i].desktop + ')');
                }
                if (arr[i].mobile.length) {
                    $tmpli.find('.playacc__tabimage--mobile').css('background-image', 'url(' + arr[i].mobile + ')');
                }
            }

            $tabImageLi = element.find('.playacc__tabimageli');

            $compile(element.find('.playacc__tabimageli'))(scope);

            $tabImageLi.on('webkitTransitionEnd otransitionend oTransitionEnd msTransitionEnd transitionend', onImageTransitionEnd);
        }

        function getTabHeights() {
            var max = (function () {
                var m = 0;
                $tabs.each(function () {
                    m = Math.max(m, $(this).height());
                });
                return Math.round(m);
            })();
            var min = Math.round((element.height() - max) / ($tabs.length - 1));
            return { min: min, max: max };
        }

        function getIsMediaXs() {
            // checking if window size is XS
            // (ugly, I know!)
            return element.css('transition').split(' ')[0] !== 'none';
        }

        init();
    };

    return {
        restrict: 'EA',
        link: link,
        scope: true
    };
}]);
app.directive('beoBuyModule', ['$rootScope', 'beoPlayAccordionService', function ($rootScope, beoPlayAccordionService) {
    var link = function (scope, element, attrs) {

        scope.onStyleChange = function (id) {
            setCart(id);

            updateGalleryThumb(id);

            // GA tracking
            var trackingObj = {
                'event': 'prodColorChange',
                'ecommerce': {
                    'currencyCode': scope.cartData.selectedProductCurrency,
                    products: [
                        {
                            'id': scope.cartData.selectedProductSKU,
                            'name': scope.productData.Name,
                            'category': scope.productData.Category,
                            'variant': scope.cartData.selectedProductStyle,
                            'price': scope.cartData.selectedProductPrice,
                            'availability': scope.cartData.selectedProductAvailability.toString().toLowerCase() === 'true' ? true : false,
                            'brand': 'Bang & Olufsen',
                            'quantity': 1,
                            'dimension10': scope.cartData.selectedProductSKU
                        }
                    ]
                }
            };

            dataLayer.push(trackingObj);
        };

        scope.toggleGallery = function ($event) {
            if (element.find('.bm__ctalink').is(':visible')) {
                $('#bm_gallery_' + scope.cartData.selectedProductId).modal();
            }
        };

        scope.onLinkClick = function ($event) {
            var href = $($event.currentTarget).attr('href');

            if (/#/i.test(href)) {

                beoPlayAccordionService.openTab(href);

                $event.preventDefault();
            }
        };

        scope.onAddToCartClick = function ($event) {
            // GA tracking
            var trackingObj = {
                'event': 'addToCart',
                'ecommerce': {
                    'currencyCode': scope.cartData.selectedProductCurrency,
                    'add': {
                        'products': [{
                            'id': scope.cartData.selectedProductSKU,
                            'name': scope.productData.Name,
                            'category': scope.productData.Category,
                            'variant': scope.cartData.selectedProductStyle,
                            'price': scope.cartData.selectedProductPrice,
                            'availability': scope.cartData.selectedProductAvailability.toString().toLowerCase() === 'true' ? true : false,
                            'brand': 'Bang & Olufsen',
                            'quantity': 1,
                            'dimension10': scope.cartData.selectedProductSKU
                        }]
                    }
                }
            };

            dataLayer.push(trackingObj);

            scope.form.submit();
        };

        function setCart(id) {
            scope.cartData.selectedProductId = scope.productData.Variants[id].ID;
            scope.cartData.selectedProductStyle = scope.productData.Variants[id].Color.Name;
            scope.cartData.selectedProductCurrency = scope.productData.Variants[id].Price.Currency;
            scope.cartData.selectedProductPrice = scope.productData.Variants[id].Price.Amount;
            scope.cartData.selectedProductCta = scope.productData.Variants[id].CtaButton.Text;
            scope.cartData.selectedProductChannelId = scope.productData.Variants[id].Price.ChannelID;
            scope.cartData.selectedProductAction = scope.productData.Variants[id].CtaButton.Url;
            scope.cartData.selectedProductMethod = scope.productData.Variants[id].CtaButton.Method;
            scope.cartData.selectedProductAvailability = scope.productData.Variants[id].CtaButton.Active;
            scope.cartData.selectedProductSKU = scope.productData.Variants[id].SKU;
        }

        function updateGalleryThumb(id) {
            scope.galleryThumbImages.filter('.active').removeClass('active').attr('aria-selected', 'false');

            scope.galleryThumbImages.eq(id).addClass('active').attr('aria-selected', 'true');
        }
    };

    return {
        restrict: 'A',
        link: link
    };
}]);

app.directive('beoBuyModuleGallery', [function () {
    var link = function (scope, element, attrs) {
        scope.margin = '0';
        scope.currSlide = 0;
        scope.slidesLength = element.find('.bm__gallery > li').length;
        scope.indexBtn = element.find('.bm__galleryindexbtn');
        scope.ratioClass = '';

        var $images = element.find('img');
        var imgDimensions = [];

        $images.one("load", function (e) {
            var $target = $(e.currentTarget);
            imgDimensions[$target.parent().index()] = { target: $target, width: e.currentTarget.width, height: e.currentTarget.height, ratio: (e.currentTarget.height / e.currentTarget.width) };

        }).each(function () {
            if (this.complete) $(this).load();
        });

        scope.slide = function (s) {
            if (s === parseInt(s, 10)) {
                scope.currSlide = s;
            } else {
                switch (s) {
                    case 'previous':
                        scope.currSlide = scope.currSlide > 0 ? scope.currSlide - 1 : 0;
                        break;
                    case 'next':
                        scope.currSlide = scope.currSlide < scope.slidesLength - 1 ? scope.currSlide + 1 : scope.slidesLength - 1;
                        break;
                    default:
                        break;
                }
            }

            console.log(scope.currSlide + "/" + scope.slidesLength);
            scope.margin = (- 1 * (scope.currSlide * 100)) + 'vw';

            updateActiveIndexButton();
        };

        element.on('hide.bs.modal', function (e) {
            $('html, body').css('overflow', '');
        }).on('hidden.bs.modal', function (e) {
            scope.currSlide = 0;

            scope.margin = '0';

            updateActiveIndexButton();

            element.parent().removeClass('zindex--top');

            scope.$apply();
        }).on('show.bs.modal', function (e) {
            $('html, body').css('overflow', 'hidden');

            element.parent().addClass('zindex--top');

            element.focus();
        });

        function updateActiveIndexButton() {
            scope.indexBtn.filter('.bm__galleryindexbtn--active').removeClass('bm__galleryindexbtn--active');

            scope.indexBtn.eq(scope.currSlide).addClass('bm__galleryindexbtn--active');
        }
    };

    return {
        restrict: 'A',
        link: link
    };
}]);

app.directive('beoNavigationTop', ['$rootScope', '$timeout', 'UtilsService', 'beoScrollToAnchorService', 'beoNavigationService', function ($rootScope, $timeout, UtilsService, beoScrollToAnchorService, beoNavigationService) {
    var link = function (scope, element, attrs) {
        var $topnavMain = element.find('#topnav_main');
        var $topnavSecondary = element.find('#topnav_secondary');
        var lastScrollTop = 0;
        var resizeTimer;
        var scrollEvent = window.document.createEvent('UIEvents');
        var isFirstRun = true;

        $(window).on('resize', onResize).trigger('resize');

        $(element).on('webkitTransitionEnd otransitionend oTransitionEnd msTransitionEnd transitionend', onTransitionEnd);

        if (Modernizr.touchevents) {
            $(window).on('scroll', onScroll);
        } else {
            $(window).on('scroll', onScroll);

            // supporting bad scroll handling in IE11
            if (!Modernizr.picture) {
                $(window).on('wheel', onScroll);
                document.addEventListener('mousedown', onMouseDown);
                document.addEventListener('mouseup', onMouseUp);
            }
        }

        $(window).trigger('scroll');

        function onMouseDown(e) {
            $(window).off('wheel', onScroll);

            $(window).on('scroll', onScroll);
        }

        function onMouseUp(e) {
            $(window).on('wheel', onScroll);

            $(window).off('scroll', onScroll);
        }

        function onScroll(e) {
            var st = window.pageYOffset || document.documentElement.scrollTop;
            if (!Modernizr.touchevents) {
                if (e.type === 'wheel' && isFirstRun) {
                    $(window).off('scroll', onScroll);
                    isFirstRun = false;
                }
            }

            if (st > lastScrollTop) {
                toggleTopNavigation('hide');
            } else {
                toggleTopNavigation('show');
            }

            if (st <= 1 && scope.fixed) {
                scope.fixed = false;
                scope.$applyAsync();
            } else if (st > 1 && !scope.fixed) {
                scope.fixed = true;
                scope.$applyAsync();
            }

            lastScrollTop = st <= 0 ? 0 : st;

            if (scope.isFoldOutOpen && scope.mediaQ === 'sm') {
                scope.onToggleFoldOut();
                scope.$apply();
            }

            setTopNavBackground();
        }

        function onTransitionEnd(e) {
            if (element.hasClass('nav--hide')) {
                element.addClass('nav--hidden');
            } else {
                element.css('margin-top', '');
            }

            element.removeClass('nav--animate-margin');
        }

        function onResizeTimeout() {
            clearResizeTimer();

            beoNavigationService.height.row1.setter($topnavMain.outerHeight());
            beoNavigationService.height.row2.setter($topnavSecondary.outerHeight());
            beoNavigationService.height.total.setter(element.outerHeight());

            $(window).on('scroll', onScroll);
        }

        function clearResizeTimer() {
            if (resizeTimer) {
                clearTimeout(resizeTimer);
                resizeTimer = null;
            }
        }

        function setTopNavBackground() {
            if ($(window).scrollTop() > 10) {
                if (!element.hasClass('nav--bg-solid')) {
                    element.addClass('nav--bg-solid');
                }
            } else {
                if (element.hasClass('nav--bg-solid')) {
                    element.removeClass('nav--bg-solid');
                }
            }
        }

        function toggleTopNavigation(toggle) {
            var height = $topnavMain.outerHeight();
            switch (toggle) {
                case 'show':
                    if (element.hasClass('nav--hide')) {
                        element.removeClass('nav--hide nav--hidden').addClass('nav--animate-margin').css('margin-top', '0');
                        beoNavigationService.marginTop.setter(0);
                    }
                    break;
                case 'hide':
                    if (!element.hasClass('nav--hide')) {
                        element.addClass('nav--animate-margin nav--hide').css('margin-top', (- 1 * height) + 'px');
                        beoNavigationService.marginTop.setter(-1 * height);

                    }
                    break;
                default:
            }
        }

        scope.onAnchorClick = function ($event) {
            var title = $($event.currentTarget).find('.navanchors__listitem--anchortitle').text();
            var scrollToId = $($event.currentTarget).attr('data-anchor-id');
            var isTopmenuAnchorLink = UtilsService.hasAttr($($event.currentTarget), 'data-anchor-id');

            scope.anchorTitle = title;

            scope.isMobileAnchorDropdownOpen = false;

            beoScrollToAnchorService.topMenuScrollToAnchor($event, scrollToId);

            if (isTopmenuAnchorLink) {
                $event.preventDefault();
            }
        };

        ///////////////////////////////////////////////
        // foldout
        ///////////////////////////////////////////////
        scope.foldOutSlider.on('webkitTransitionEnd otransitionend oTransitionEnd msTransitionEnd transitionend', onFoldOutTransitionEnd);

        scope.onFoldOutSwipe = function (dir) {
            switch (dir) {
                case 'right':
                    break;
                case 'left':
                    scope.onToggleFoldOut();
                    break;
                default:
            }
        };

        $('.nav__burger').on('keyup', function (e) {
            if (e.which === 13) {
                scope.onToggleFoldOut(true);
            }
        });
        scope.onToggleFoldOut = function (apply) {
            scope.isFoldOutOpen = !scope.isFoldOutOpen;

            if ($rootScope.isPhone || $rootScope.isTablet) {
                $rootScope.overflowHiddenY = !$rootScope.overflowHiddenY;
            }

            if (scope.isFoldOutOpen) {
                scope.isFoldOutOpening = true;
            }

            if (!scope.isFoldOutOpen) {
                scope.isFoldOutClosing = true;
                scope.isFoldOutOpened = false;
            }

            if (apply) {
                scope.$apply();
            }
        };

        function onFoldOutTransitionEnd(e) {
            if (scope.isFoldOutClosing) {
                scope.isFoldOutClosing = false;
            }

            if (scope.isFoldOutOpening) {
                scope.isFoldOutOpening = false;
                scope.isFoldOutOpened = true;
            }

            scope.$apply();
        }

        function onResize(e) {
            if (element.hasClass('nav--hidden')) {
                element.css('margin-top', -1 * $topnavMain.outerHeight() + 'px');
            }

            if (scope.foldOutSlider.css('border-top-style') === 'solid') {
                scope.mediaQ = 'xs';
            }

            if (scope.foldOutSlider.css('border-top-style') === 'dotted') {
                scope.mediaQ = 'sm';
            }

            if (scope.foldOutSlider.css('border-top-style') === 'dashed') {
                scope.mediaQ = 'md';
            }

            if (scope.mediaQ === 'xs' || scope.mediaQ === 'sm') {
                scope.foldOutHeight = element.find('#nav_foldout_slider .nav__list').outerHeight();
            } else {
                scope.foldOutHeight = '';
                scope.isFoldOutOpen = false;
                scope.isFoldOutClosing = false;
                scope.isFoldOutOpening = false;
                scope.isFoldOutOpened = false;
            }

            scope.$applyAsync();

            clearResizeTimer();

            resizeTimer = setTimeout(onResizeTimeout, 500);

            $(window).off('scroll', onScroll);
        }
    };

    return {
        restrict: 'EA',
        link: link
    };
}]);
app.service('UtilsService', function ($rootScope, $window) {

    return {
        lastScrollTop: 0,
        getScrollDirection: function () {
            var d;
            var st = window.pageYOffset;
            if (st > this.lastScrollTop) {
                d = "down";
            } else {
                d = "up";
            }
            this.lastScrollTop = st;
            return d;
        },

        getDomain: function () {
            var url = window.location.href
            var arr = url.split("/");
            var result = arr[0] + "//" + arr[2]

            return result;
        },

        getIsUrlRelative: function (url) {
            var isAbsolute = new RegExp('^([a-z]+://|//)', 'i');
            return !isAbsolute.test(url);
        },

        getStringNumberUnit: function (str) {
            if (str.indexOf('%') > -1) {
                return '%';
            } else if (str.indexOf('px') > -1) {
                return 'px';
            } else {
                return 'undefined';
            }
        },

        getTagContent: function (str, tag) {
            var matchPattern = '<' + tag + '>(.*?)<\/' + tag + '>';
            var matchRegex = new RegExp(matchPattern);
            var replacePattern = '<\/?' + tag + '>';
            var replaceRegex = new RegExp(replacePattern, 'g');
            return str.match(matchRegex).map(function (val) { return val.replace(replaceRegex, ''); })[0];
        },

        // { image: element, context: element_container, orgwidth: image_original_width, orgheight: image_original_height };
        getFullscreenImageDimensions: function (o) {
            var $parent = angular.element(o.context);
            var widthRatio = $parent.width() / o.orgwidth;
            var heightRatio = $parent.height() / o.orgheight;
            var ratio = widthRatio;
            var marginTop;
            var marginLeft;
            var width;
            var height;

            if (widthRatio * o.orgheight < $parent.height()) {
                ratio = heightRatio;
            }

            width = o.orgwidth * ratio;
            height = o.orgheight * ratio;

            if (widthRatio * o.orgheight < $parent.height()) {
                ratio = heightRatio;
                marginTop = 0;
                marginLeft = -1 * (((o.orgwidth * ratio) - $parent.width()) / 2);
            } else {
                marginTop = -1 * (((o.orgheight * ratio) - $parent.height()) / 2);
                marginLeft = 0;
            }

            return { width: width, height: height, marginLeft: marginLeft, marginTop: marginTop };
        },

        getStringAsNumber: function (num) {
            return Number((num.split('px'))[0]);
        },

        getURLParameter: function (sParam) {
            var sPageURL = decodeURIComponent(window.location.search.substring(1)),
                sURLVariables = sPageURL.split('&'),
                sParameterName,
                i;

            for (i = 0; i < sURLVariables.length; i++) {
                sParameterName = sURLVariables[i].split('=');
                if (sParameterName[0] === sParam) {
                    return sParameterName[1] === undefined ? true : sParameterName[1];
                }
            }
        },

        verticalScrollbarEnabled: function () {
            return $('.content-wrp').height() > $window.innerHeight;
        },

        setTimeoutToNull: function (t) {
            if (t) {
                clearInterval(t);
                t = null;
            }
        },

        setIntervalToNull: function (i) {
            if (i) {
                clearInterval(i);
                i = null;
            }
        },

        getScrollBarWidth: function () {
            var inner = document.createElement('p');
            inner.style.width = "100%";
            inner.style.height = "200px";

            var outer = document.createElement('div');
            outer.style.position = "absolute";
            outer.style.top = "0px";
            outer.style.left = "0px";
            outer.style.visibility = "hidden";
            outer.style.width = "200px";
            outer.style.height = "150px";
            outer.style.overflow = "hidden";
            outer.appendChild(inner);

            document.body.appendChild(outer);
            var w1 = inner.offsetWidth;
            outer.style.overflow = 'scroll';
            var w2 = inner.offsetWidth;
            if (w1 == w2) w2 = outer.clientWidth;

            document.body.removeChild(outer);
            return (w1 - w2);
        },

        sortBy: function (field, reverse, primer) {
            var key = primer ? function (x) { return primer(x[field]) } : function (x) { return x[field] };
            reverse = !reverse ? 1 : -1;
            return function (a, b) {
                return a = key(a), b = key(b), reverse * ((a > b) - (b > a));
            }
        },

        // round to decimals (n = number to round, n = decimals (10, 100, 1000, 10000))
        dec: function (n, r) {
            return Math.round(r * n) / r;
        },

        distance: function (lat1, lon1, lat2, lon2, unit) {
            var radlat1 = Math.PI * lat1 / 180;
            var radlat2 = Math.PI * lat2 / 180;
            var radlon1 = Math.PI * lon1 / 180;
            var radlon2 = Math.PI * lon2 / 180;
            var theta = lon1 - lon2;
            var radtheta = Math.PI * theta / 180;
            var dist = Math.sin(radlat1) * Math.sin(radlat2) + Math.cos(radlat1) * Math.cos(radlat2) * Math.cos(radtheta);
            dist = Math.acos(dist);
            dist = dist * 180 / Math.PI;
            dist = dist * 60 * 1.1515;
            if (unit.toLowerCase() == "k") {
                dist = dist * 1.609344;
            }
            if (unit.toLowerCase() == "n") {
                dist = dist * 0.8684;
            }
            return dist
        },

        formatDistance: function (d) {
            var unitStr = d < 1 ? ' m' : ' km';
            var dist = d < 1 ? this.formatMeters(d) : this.dec(d, 10);
            return dist + unitStr;
        },

        formatMeters: function (d) {
            var dist = ((this.dec(d, 1000)).toString()).substr(2);
            while (dist.charAt(0) === '0') {
                dist = dist.substr(1);
            }
            return dist;
        },

        stringToBool: function (str) {
            return str == 'true' ? true : false;
        },

        hasAttr: function ($t, attr) {
            var attr2 = $t.attr(attr);
            return typeof attr2 !== typeof undefined && attr2 !== false;
        }
    };
});
app.factory("GlobalService", ['$rootScope', function ($rootScope) {

    var global = {
        bodyOverflow: {
            setter: null,
            getter: null,
            value: false,
        }
    };

    global.bodyOverflow.setter = function (newValue) {
        global.bodyOverflow.value = newValue;

        $rootScope.$broadcast('global:updated', global);
    }

    global.bodyOverflow.getter = function () {
        return global.bodyOverflow.value;
    }

    return global;
}]);
app.service('beoScrollToAnchorService', function ($rootScope, UtilsService) {
    this.topMenuScrollToAnchor = function (e, id, callback, context) {
        var desktopTopMenuHeight = $rootScope.isPhone ? $('#top_menu').children().outerHeight() : $('#top_menu').outerHeight();
        var $section,
            topmenuHeight,
            sectionOffsetTop,
            offset,
            cookiebarHeight,
            anhorIdAttr,
            isTopmenuAnchorLink,
            $target = $(e.currentTarget),
            hrefSplitArr,
            scrollTo,
            scrollSpeed = 1000,
            scrollSpeedRelative,
            currentScrollTop,
            posDiff;

        // check if link is top menu link. Only top menu links have an anchor ID
        isTopmenuAnchorLink = UtilsService.hasAttr($target, 'data-anchor-id');

        // set anchor section value
        hrefSplitArr = isTopmenuAnchorLink ? [] : $target.attr('href').split('#');
        anhorIdAttr = isTopmenuAnchorLink ? $target.attr('data-anchor-id') : hrefSplitArr[1];

        // cookie bar height for offsetting targetsection top position
        cookiebarHeight = $('#cookiebar ').length ? $('#cookiebar ').height() : 0;

        // remove any unwanted whitespaces in anchor ID
        anhorIdAttr = anhorIdAttr ? stripWhiteSpace(anhorIdAttr) : '';

        // set targetsection if anchor ID is set
        $section = anhorIdAttr.length > 0 ? $('#' + anhorIdAttr) : null;

        // topmenu height for offsetting targetsection top position
        topmenuHeight = desktopTopMenuHeight;

        // section position from top

        sectionOffsetTop = $section && $section.length ? $section.offset().top : 0;


        // calculated offset
        offset = sectionOffsetTop > window.pageYOffset ? cookiebarHeight + (desktopTopMenuHeight / 2) : cookiebarHeight + desktopTopMenuHeight;

        // disable submenu while scroll is animating
        $rootScope.enableUpdatedAnchorMenu = false;

        scrollTo = sectionOffsetTop - offset;

        // calculating relative scroll speed according to current
        // scroll position and target scroll position
        currentScrollTop = $(window).scrollTop();
        posDiff = Math.max(currentScrollTop, scrollTo) - Math.min(currentScrollTop, scrollTo);

        scrollSpeedRelative = (posDiff / $(document).height()) * 5000;
        console.log('0');
        if (isTopmenuAnchorLink) {

            if ($section && !$rootScope.preventAnchorClick && !$target.parent().hasClass('active')) {
                $('html, body').stop().animate({ scrollTop: scrollTo }, scrollSpeed)
                    .promise()
                    .then(function () {
                        // update active menu item in anchor menu
                        $target.parent().siblings().removeClass('active');
                        $target.parent().addClass('active');

                        $rootScope.$broadcast('anchorScrollComplete');

                        $rootScope.enableUpdatedAnchorMenu = true;
                    });
            }

            $rootScope.$broadcast('anchorScroll');

        } else {
            if ($section && hrefSplitArr.length > 1) {
                // if current page anchor link, scroll to anchor
                $('html, body').stop().animate({ scrollTop: scrollTo }, scrollSpeed)
                    .promise()
                    .then(function () {

                        $rootScope.$broadcast('anchorScrollComplete');

                        $rootScope.enableUpdatedAnchorMenu = true;
                    });
                e.preventDefault();
                $rootScope.$broadcast('anchorScroll');

            } else {
                // if page change, go to page
                window.location.href = hrefSplitArr[0];
            }
        }

        function stripWhiteSpace(str) {
            return str.replace(/\s/g, '');
        }

        if (callback != null) {
            callback();
        }

    };
});

app.service('beoDialogBox', function ($http) {
    this.alert = function (text, callback) {
        // plugin docs:
        // http://bootboxjs.com
        bootbox.alert(text, function () {
            if (callback != null) {
                callback();
            }
        });
    };
});

/// <reference path="../../Vendor/Jquery/jquery-1.11.3.js" />
app.factory('ImageCacheFactory', function ($q, $timeout) {
    return {
        Cache: function (urls) {
            var promises = [];
            for (var i = 0; i < urls.length; i++) {
                if (!urls[i]) { break; } // skip if empty
                var deferred = $q.defer();
                var img = new Image();
                img.onload = (function (deferred) {
                    return function () {
                        deferred.resolve();
                    };
                })(deferred);

                img.onerror = (function (deferred, url) {
                    return function () {
                        deferred.reject(url);
                    };
                })(deferred, urls[i]);

                promises.push(deferred.promise);
                img.src = urls[i];
            }
            return $q.all(promises);
        }
    };
});
app.service('scopeService', function () {
    return {
        safeApply: function ($scope, fn) {
            var phase = $scope.$root.$$phase;
            if (phase == '$apply' || phase == '$digest') {
                if (fn && typeof fn === 'function') {
                    fn();
                }
            } else {
                $scope.$apply(fn);
            }
        },
    };
});
app.service('beoStorefinderService', ['$q', '$http', 'UtilsService', function ($q, $http, UtilsService) {
    this.getStores = function (obj) {
        var url = '/api/stores/get';
        var urlPostfix = obj ? (obj.id || obj.filterid ? '/?' : '') : '';
        finalUrl = '';

        urlPostfix = obj && obj.id ? urlPostfix + 'id=' + obj.id : urlPostfix;
        urlPostfix = obj && obj.filterid ? urlPostfix + 'filterid=' + obj.filterid : urlPostfix;

        finalUrl = url + urlPostfix

        return $http.get(finalUrl);
    };

    this.getCountries = function () {
        var url = '/api/stores/getcountries';
        return $http.get(url);
    };

    // Use Google Maps style genertor for styling:
    // http://www.mapstylr.com/map-style-editor/
    this.getStyle = function () {
        var style = [{ "featureType": "landscape", "elementType": "geometry", "stylers": [{ "color": "#f2f2f2" }] }, { "featureType": "landscape.man_made", "elementType": "geometry", "stylers": [{ "color": "#f2f2f2" }] }, { "featureType": "landscape.natural", "elementType": "geometry", "stylers": [{ "color": "#f2f2f2" }, { "weight": 0.1 }] }, { "featureType": "poi", "elementType": "geometry", "stylers": [{ "color": "#f2f2f2" }] }, { "featureType": "water", "elementType": "geometry", "stylers": [{ "color": "#4f586e" }] }, { "featureType": "water", "elementType": "labels.text.fill", "stylers": [{ "visibility": "on" }, { "color": "#f2f2f2" }] }, { "featureType": "water", "elementType": "labels.text.stroke", "stylers": [{ "visibility": "off" }] }, { "featureType": "road.highway", "elementType": "geometry.fill", "stylers": [{ "color": "#d6d6d6" }] }, { "featureType": "road.highway", "elementType": "geometry.stroke", "stylers": [{ "visibility": "off" }] }, { "featureType": "road.arterial", "elementType": "geometry.fill", "stylers": [{ "color": "#ffffff" }] }, { "featureType": "road.arterial", "elementType": "geometry.stroke", "stylers": [{ "visibility": "off" }] }, { "featureType": "road.local", "elementType": "geometry.fill", "stylers": [{ "color": "#ffffff" }] }, { "featureType": "road.local", "elementType": "geometry.stroke", "stylers": [{ "visibility": "off" }] }, { "featureType": "transit", "elementType": "all", "stylers": [{ "visibility": "off" }] }, { "featureType": "administrative", "elementType": "labels.text.stroke", "stylers": [{ "visibility": "off" }] }, { "featureType": "administrative", "elementType": "labels.text.fill", "stylers": [{ "color": "#a2a2a2" }, { "weight": 0.1 }] }, { "featureType": "administrative", "elementType": "labels.text.stroke", "stylers": [{ "visibility": "off" }] }, { "featureType": "poi", "elementType": "labels.text.stroke", "stylers": [{ "visibility": "off" }] }, { "featureType": "poi", "elementType": "labels.text.fill", "stylers": [{ "color": "#434343" }] }, { "featureType": "road.arterial", "elementType": "labels.icon", "stylers": [{ "lightness": -87 }, { "gamma": 9.99 }] }, { "featureType": "administrative.locality", "elementType": "labels.text.fill", "stylers": [{ "color": "#2f2f2f" }] }, { "featureType": "administrative.locality", "elementType": "labels.text.stroke", "stylers": [{ "visibility": "on" }, { "color": "#f2f2f2" }, { "weight": 1.54 }] }, { "featureType": "administrative.country", "elementType": "labels.text.stroke", "stylers": [{ "visibility": "on" }, { "color": "#f2f2f2" }, { "weight": 1.28 }] }, { "featureType": "administrative.country", "elementType": "labels.text.fill", "stylers": [{ "color": "#2f2f2f" }] }, { "featureType": "road", "elementType": "labels.icon", "stylers": [{ "saturation": -100 }, { "lightness": 56 }] }];
        return style;
    };

    this.getInfo = function (store, currlocation) {
        var openingHours = this.getOpeningHoursArray(store.OpeningHours);
        var info = '<div class="beo-map-infowindow-bg"><div tabindex="0" class="beo-map-infowindow"><div class="beo-map-infowindow-inner"><a aria-label="Close infobox" role="button" tabindex="0" class="beo-map-infowindow-close underline-none"><span class="bo-icon-close"></span></a>';

        info += '<div class="beo-map-infowindow-section">';
        info += !isNull(store.Address1) ? '<h5>' + store.Address1 + '</h5>' : '';
        info += !isNull(store.Address2) ? '<p class="small">' + store.Address2 + '</p>' : '';
        info += !isNull(store.Address3) ? '<p class="small">' + store.Address3 + '</p>' : '';
        info += !isNull(store.Address4) ? '<p class="small">' + store.Address4 + '</p>' : '';
        info += !isNull(store.ZipCode) || !isNull(store.City) ? '<p class="small">' : '';
        info += !isNull(store.ZipCode) ? store.ZipCode + ' ' : '';
        info += !isNull(store.City) ? store.City : '';
        info += !isNull(store.ZipCode) || !isNull(store.City) ? '</p>' : '';
        info += '</div>';
        if (currlocation) {

            info += !isNull(store.DistanceStr) ? '<div class="beo-map-infowindow-section"><p class="small"><span class="bo-icon-pin2"></span><span>' + this.getDistanceFromSearch(currlocation, { lat: store.Latitude, lng: store.Longitude }) + '</span></p></div>' : '';
        }

        if (openingHours.length) {
            info += '<div class="beo-map-infowindow-section">';
            for (var i in openingHours) {
                info += '<p class="smallest">' + openingHours[i].day + ': <strong>' + openingHours[i].hours + '</strong></p>';
            }
            info += '</div>';
        }

        info += !isNull(store.Phone) ? '<div class="beo-map-infowindow-section"><p class="small">Phone:<br /><strong><a aria-label="' + store.Address1 + ' Phone ' + store.Phone + '" tabindex="0" class="underline-none" href="tel:' + store.Phone + '">' + store.Phone + '</a></strong></p></div>' : '';
        info += !isNull(store.Email) ? '<div class="beo-map-infowindow-section"><p class="small word-break-all">Mail:<br /><a aria-label="' + store.Address1 + ' Email ' + store.Email + '" tabindex="0" class="underline-none" href="mailto:' + store.Email + '"><strong>' + store.Email + '</strong></a></p></div>' : '';
        info += '<div class="beo-map-infowindow-visit beo-map-infowindow-section">';
        info += '<a tabindex="0" aria-label="' + store.Address1 + ' ' + window.labelVisitWebsite + '" data-storeid="' + store.ID + '" data-retailertypeid="' + store.RetailerTypeID + '" href="' + store.HomepageUrl + '" class="button solid smaller grey beo-map-infowindow-visit-website-btn" target="_blank" data-type="visit_website">' + (window.labelVisitWebsite ? window.labelVisitWebsite : '&nbsp;') + '</a>';
        info += '<a tabindex="0" aria-label="' + store.Address1 + ' ' + window.labelBookADemo + '" data-storeid="' + store.ID + '" data-retailertypeid="' + store.RetailerTypeID + '" href="' + store.BookDemoUrl + '" class="button solid smaller grey beo-map-infowindow-bookdemo-btn" data-type="book_demo">' + (window.labelBookADemo ? window.labelBookADemo : '&nbsp;') + '</a>';
        info += '</div>';
        info += '<div class="beo-map-infowindow-directions beo-map-infowindow-section">';
        if (currlocation) {
            info += '<a tabindex="0" aria-label="' + store.Address1 + ' ' + window.labelGetDirections + '" data-storeid="' + store.ID + '" data-retailertypeid="' + store.RetailerTypeID + '" target="_blank" href="http://maps.google.com/?saddr=' + currlocation.lat + ',' + currlocation.lng + '&daddr=' + store.Latitude + ',' + store.Longitude + '" data-type="get_directions" class="button solid smaller beo-map-infowindow-directions-btn">' + (window.labelGetDirections ? window.labelGetDirections : '&nbsp;') + '</a>';
        }
        info += '</div></div></div></div>';
        return info;
    };

    function isNull(str) {
        return str == null;
    };

    this.getOpeningHoursArray = function (obj) {
        var arr = [];
        var i = 0;
        for (var val in obj) {
            if (obj[val] != null && obj[val] != '') {
                arr[i] = { day: val, hours: obj[val] };
            }
            i++;
        }

        return arr;
    };

    this.isStoreinClosestStoresList = function (scope, storeid) {
        return $.grep(scope.closestStores, function (e) { return e.ID == storeid; }).length > 0 ? true : false;
    };

    this.getReversedGecode = function (address, country) {
        var geocoder = new google.maps.Geocoder();
        var deferred = $q.defer();
        var options = country != null ? { 'address': address, componentRestrictions: { country: country } } : { 'address': address };

        geocoder.geocode(options, function (results, status) {
            if (status === google.maps.GeocoderStatus.OK) {
                return deferred.resolve(results[0]);
            } else {
                deferred.reject({ error: status });
            }
        });

        return deferred.promise;
    };

    this.getAreaBounds = function (location) {
        var geocoder = new google.maps.Geocoder();
        var deferred = $q.defer();

        geocoder.geocode({ 'address': location }, function (results, status) {
            if (status == google.maps.GeocoderStatus.OK) {

                return deferred.resolve({
                    center: results[0].geometry.location,
                    bounds: results[0].geometry.viewport
                });

            } else {
                deferred.reject({ error: status });
            }
        });
        return deferred.promise;
    };

    this.getStoresOrderedByDistance = function (lat, lng, storeslist, restrictISO) {
        var arr = [];
        for (var i = 0; i < storeslist.length; i++) {
            var store = storeslist[i];
            var distanceFromSearch;
            distanceFromSearch = UtilsService.distance(lat, lng, store.Latitude, store.Longitude, 'K');
            arr[i] = store;
            arr[i].Distance = UtilsService.dec(distanceFromSearch, 10);
            arr[i].DistanceStr = UtilsService.formatDistance(distanceFromSearch);
        }
        return arr.sort(UtilsService.sortBy('Distance', false, parseInt));
    }

    this.getClosestStoresSimple = function (arr, maxlength) {
        var tempArr = [];
        var maxlength = arr.length < maxlength ? arr.length : maxlength;
        for (var i = 0; i < maxlength; i++) {
            tempArr.push(arr[i]);
        }
        return tempArr;
    }

    this.getDistanceFromSearch = function (fromcoords, tocoords) {
        var distance = UtilsService.distance(fromcoords.lat, fromcoords.lng, tocoords.lat, tocoords.lng, 'K');
        return UtilsService.formatDistance(distance);
    }

}]);

app.service('beoSubmitEow', function ($http,$q) {
    function send(obj) {
        var request = $http({
            method: "post",
            url: "/api/ExtendWarranty/submit",
            data: {
                Key: obj.key,
                CustomerName: obj.name,
                CustomerPhone: obj.phone,
                CustomerComment: obj.comment
            }
        });
        return (request.then(handleSuccess, handleError));
    }

    function handleSuccess(response) {
        return (response.data);
    }

    function handleError(response) {
        if (
            !angular.isObject(response.data) ||
            !response.data.message
            ) {
            return ($q.reject("An unknown error occurred."));
        }
        
        return ($q.reject(response.data.message));
    }

    return ({
        send: send
        
    });
});

app.service('beoSubmitBookDemo', function ($http, $q) {

    function send(obj) {
        var request = $http({
            method: "post",
            url: "/api/bookdemo/submit",
            data: {
                storeid: obj.storeid,
                salutation: obj.title,
                firstname: obj.firstname,
                middlename: obj.middlename,
                lastname: obj.lastname,
                phone: obj.phone,
                email: obj.email,
                comment: obj.comment
            }
        });
        return (request.then(handleSuccess, handleError));
    }

    function handleSuccess(response) {
        return (response.data);
    }

    function handleError(response) {
        if (
            !angular.isObject(response.data) ||
            !response.data.message
            ) {
            return ($q.reject("An unknown error occurred."));
        }

        return ($q.reject(response.data.message));
    }

    return ({
        send: send

    });
});

app.service("beoSearchService", function ($http, $q) {
    function send(term, max) {
        var deferredAbort = $q.defer();
        var request = $http({
            method: 'post',
            url: '/api/search/query?term=' + term + '&maxcount=' + max,
            timeout: deferredAbort.promise
        });

        var promise = request.then(
            function (response) {
                return (response.data);
            },
            function (response) {
                return ($q.reject(response));
            }
        );

        promise.abort = function () {
            deferredAbort.resolve();
        };

        promise.finally(
            function () {
                promise.abort = angular.noop;
                deferredAbort = request = promise = null;
            }
        );
        return (promise);
    }

    return ({
        send: send
    });
});
app.service('beoDialogBox', function ($http) {
    this.alert = function (text, callback) {
        // plugin docs:
        // http://bootboxjs.com
        bootbox.alert(text, function () {
            if (callback != null) {
                callback();
            }
        });
    };
});

// Newsletter signup
app.service('beoNewsletterSignupService', function ($http, $q) {
    function sendNewsletterSignup(obj) {
        var request = $http({
            method: "post",
            url: "/api/newsletterapi/subscribe",
            data: {
                FormId: obj.formid,
                Firstname: obj.firstname,
                Lastname: obj.lastname,
                Email: obj.email,
                Nationality: obj.nationality,
                DmCode: obj.dmcode ? obj.dmcode : null
            }
        });

        return (request.then(handleSuccess, handleError));
    }

    function sendNewsletterDmSignup(obj) {
        var request = $http({
            method: "post",
            url: "/api/GenericPermissionCollection/subscribe",
            data: {
                FormId: obj.formid,
                Firstname: obj.firstname,
                Lastname: obj.lastname,
                Email: obj.email,
                Nationality: obj.nationality,
                DmCode: obj.dmcode ? obj.dmcode : null
            }
        });

        return (request.then(handleSuccess, handleError));
    }

    function handleSuccess(response) {
        return (response.data);
    }

    function handleError(response) {
        return (response);
    }

    return ({
        sendNewsletterSignup: sendNewsletterSignup,
        sendNewsletterDmSignup: sendNewsletterDmSignup
    });
});

app.service('beoNewsletterUnsubscribeService', function ($http, $q) {
    function sendNewsletterUnsubscribe(obj) {
        var request = $http({
            method: "post",
            url: "/api/newsletterapi/unsubscribe",
            data: {
                FormId: obj.formid,
                Email: obj.email,
                ReasonToUnsubscribe: obj.reason ? obj.reason : null
            }
        });
        return (request.then(handleSuccess, handleError));
    }

    function sendNewsletterDmUnsubscribe(obj) {
        var request = $http({
            method: "post",
            url: "/api/GenericPermissionCollection/unsubscribe",
            data: {
                FormId: obj.formid,
                Email: obj.email,
                ReasonToUnsubscribe: obj.reason ? obj.reason : null
            }
        });
        return (request.then(handleSuccess, handleError));
    }

    function handleSuccess(response) {
        return (response.data);
    }

    function handleError(response) {
        return (response);
    }

    return ({
        sendNewsletterUnsubscribe: sendNewsletterUnsubscribe,
        sendNewsletterDmUnsubscribe: sendNewsletterDmUnsubscribe

    });
});

// I provide a utility class for preloading image objects.
app.factory("beoLoadImage", function ($q, $rootScope) {
    // I manage the preloading of image objects. Accepts an array of image URLs.
    function Preloader(imageLocations) {
        // I am the image SRC values to preload.
        this.imageLocations = imageLocations;
        // As the images load, we'll need to keep track of the load/error
        // counts when announing the progress on the loading.
        this.imageCount = this.imageLocations.length;
        this.loadCount = 0;
        this.errorCount = 0;
        // I am the possible states that the preloader can be in.
        this.states = {
            PENDING: 1,
            LOADING: 2,
            RESOLVED: 3,
            REJECTED: 4
        };
        // I keep track of the current state of the preloader.
        this.state = this.states.PENDING;
        // When loading the images, a promise will be returned to indicate
        // when the loading has completed (and / or progressed).
        this.deferred = $q.defer();
        this.promise = this.deferred.promise;
    }
    // ---
    // STATIC METHODS.
    // ---
    // I reload the given images [Array] and return a promise. The promise
    // will be resolved with the array of image locations.
    Preloader.preloadImages = function (imageLocations) {
        var preloader = new Preloader(imageLocations);
        return (preloader.load());
    };
    // ---
    // INSTANCE METHODS.
    // ---
    Preloader.prototype = {
        // Best practice for "instnceof" operator.
        constructor: Preloader,
        // ---
        // PUBLIC METHODS.
        // ---
        // I determine if the preloader has started loading images yet.
        isInitiated: function isInitiated() {
            return (this.state !== this.states.PENDING);
        },
        // I determine if the preloader has failed to load all of the images.
        isRejected: function isRejected() {
            return (this.state === this.states.REJECTED);
        },
        // I determine if the preloader has successfully loaded all of the images.
        isResolved: function isResolved() {
            return (this.state === this.states.RESOLVED);
        },
        // I initiate the preload of the images. Returns a promise.
        load: function load() {
            // If the images are already loading, return the existing promise.
            if (this.isInitiated()) {
                return (this.promise);
            }
            this.state = this.states.LOADING;
            for (var i = 0 ; i < this.imageCount ; i++) {
                this.loadImageLocation(this.imageLocations[i]);
            }
            // Return the deferred promise for the load event.
            return (this.promise);
        },
        // ---
        // PRIVATE METHODS.
        // ---
        // I handle the load-failure of the given image location.
        handleImageError: function handleImageError(imageLocation) {
            this.errorCount++;
            // If the preload action has already failed, ignore further action.
            if (this.isRejected()) {
                return;
            }
            this.state = this.states.REJECTED;
            this.deferred.reject(imageLocation);
        },
        // I handle the load-success of the given image location.
        handleImageLoad: function handleImageLoad(imageLocation) {
            this.loadCount++;
            // If the preload action has already failed, ignore further action.
            if (this.isRejected()) {
                return;
            }
            // Notify the progress of the overall deferred. This is different
            // than Resolving the deferred - you can call notify many times
            // before the ultimate resolution (or rejection) of the deferred.
            this.deferred.notify({
                percent: Math.ceil(this.loadCount / this.imageCount * 100),
                imageLocation: imageLocation
            });
            // If all of the images have loaded, we can resolve the deferred
            // value that we returned to the calling context.
            if (this.loadCount === this.imageCount) {
                this.state = this.states.RESOLVED;
                this.deferred.resolve(this.imageLocations);
            }
        },
        // I load the given image location and then wire the load / error
        // events back into the preloader instance.
        // --
        // NOTE: The load/error events trigger a $digest.
        loadImageLocation: function loadImageLocation(imageLocation) {
            var preloader = this;
            // When it comes to creating the image object, it is critical that
            // we bind the event handlers BEFORE we actually set the image
            // source. Failure to do so will prevent the events from proper
            // triggering in some browsers.
            var image = $(new Image())
                .load(
                    function (event) {
                        // Since the load event is asynchronous, we have to
                        // tell AngularJS that something changed.
                        $rootScope.$apply(
                            function () {
                                preloader.handleImageLoad(event.target.src);
                                // Clean up object reference to help with the
                                // garbage collection in the closure.
                                preloader = image = event = null;
                            }
                        );
                    }
                )
                .error(
                    function (event) {
                        // Since the load event is asynchronous, we have to
                        // tell AngularJS that something changed.
                        $rootScope.$apply(
                            function () {
                                preloader.handleImageError(event.target.src);
                                // Clean up object reference to help with the
                                // garbage collection in the closure.
                                preloader = image = event = null;
                            }
                        );
                    }
                )
                .prop("src", imageLocation)
            ;
        }
    };
    // Return the factory instance.
    return (Preloader);
});
app.service('beoReserveService', function ($http, $q) {
    function submit(obj) {
        var deferredAbort = $q.defer();

        var request = $http({
            method: 'post',
            url: '/api/ProductReservation/Submit',
            timeout: deferredAbort.promise,
            'data': obj,
        });

        var promise = request.then(
            function (response) {
                return (response.data);
            },
            function (response) {
                return ($q.reject(response));
            }
        );

        promise.abort = function () {
            deferredAbort.resolve();
        };

        promise.finally(
            function () {
                promise.abort = angular.noop;
                deferredAbort = request = promise = null;
            }
        );
        return (promise);
    }

    function getCities(country, filterid, formid) {
        var deferred = $q.defer();

        function onGetCitiesSuccess(data) {
            deferred.resolve(data);
        }

        function onGetCitiesError(msg, code) {
            deferred.reject(msg);
        }

        $http.get('/api/stores/getcitiesbycountry?country=' + country + '&FilterID=' + filterid + '&FormID=' + formid).success(onGetCitiesSuccess).error(onGetCitiesError);

        return deferred.promise;
    }

    function getStores(city, filterid, formid) {
        var deferred = $q.defer();

        function onGetStoresSuccess(data) {
            deferred.resolve(data);
        }

        function onGetStoresError(msg, code) {
            deferred.reject(msg);
        }

        $http.get('/api/stores/GetStoresByCity?city=' + city + '&FilterID=' + filterid + '&FormID=' + formid).success(onGetStoresSuccess).error(onGetStoresError);

        return deferred.promise;
    }

    return ({
        submit: submit,
        getCities: getCities,
        getStores: getStores
    });
});
app.service('beoLockScrollposition', function ($rootScope) {
    this.lockScrollPosition = function (toggle, obj) {
        if (toggle) {

            $rootScope.tmpScrollPos = $(window).scrollTop();

            $(window).scrollTop(0);

            $('body').addClass('lock-scrollposition');
        } else {
            $('body').removeClass('lock-scrollposition');

            $('#top_menu').addClass('navbar__nav--nocollapse');

            $(window).scrollTop($rootScope.tmpScrollPos);

            setTimeout(function () {
                $('#top_menu').css('margin-top', '').removeClass('navbar__nav--nocollapse');
            }, 50);
        }
    };
});

app.service('beoPermissionSubscribeService', function ($http, $q) {
    function submitPermissionSubscribe(formdata) {
        var request = $http({
            method: "post",
            url: "/api/Permission/subscribe",
            data: {
                FormId: formdata.formid,
                Firstname: formdata.firstname,
                Lastname: formdata.lastname,
                Email: formdata.email,
                country: formdata.country,
                DmCode: formdata.dmCode ? formdata.dmCode : null
            }
        });
        return (request.then(handleSuccess, handleError));
    }

    function submitPermissionUnsubscribe(formdata) {
        console.table(formdata);
        var request = $http({
            method: "post",
            url: "/api/Permission/unsubscribe",
            data: {
                FormId: formdata.formid,
                Email: formdata.email,
                ReasonToUnsubscribe: formdata.reasonToUnsubscribe ? formdata.reasonToUnsubscribe : null,
                Key: formdata.key
            }
        });
        return (request.then(handleSuccess, handleError));
    }

    function handleSuccess(response) {
        return (response.data);
    }

    function handleError(response) {
        return (response);
    }

    return ({
        submitPermissionSubscribe: submitPermissionSubscribe,
        submitPermissionUnsubscribe: submitPermissionUnsubscribe
    });
});

app.factory('beoNavigationService', ['$rootScope', function ($rootScope) {
    var nav = {
        marginTop: {
            setter: null,
            getter: null,
            value: false
        },
        height: {
            row1: {
                setter: null,
                getter: null,
                value: false
            },
            row2: {
                setter: null,
                getter: null,
                value: false
            },
            total: {
                setter: null,
                getter: null,
                value: false
            }
        }
    };

    nav.marginTop.setter = function (newValue) {
        nav.marginTop.value = newValue;
    };

    nav.marginTop.getter = function () {
        return nav.marginTop.value;
    };

    nav.height.row1.setter = function (newValue) {
        nav.height.row1.value = newValue;
    };

    nav.height.row1.getter = function () {
        return nav.height.row1.value;
    };

    nav.height.row2.setter = function (newValue) {
        nav.height.row2.value = newValue;
    };

    nav.height.row2.getter = function () {
        return nav.height.row2.value;
    };

    nav.height.total.setter = function (newValue) {
        nav.height.total.value = newValue;
    };

    nav.height.total.getter = function () {
        return nav.height.total.value;
    };

    return nav;
}]);
app.factory('beoPlayAccordionService', ['$rootScope', 'beoNavigationService', 'UtilsService', function ($rootScope, beoNavigationService, UtilsService) {
    var playacc = {};

    playacc.openTab = function (h) {
        var direction = $(h).offset().top < $(window).scrollTop() ? 'up' : 'down';

        if ($(h).length) {
            if ($(h).hasClass('playacc__tab')) {
                var end = direction === 'up' ? $(h).offset().top - beoNavigationService.height.total.getter() : $(h).offset().top - beoNavigationService.height.row1.getter();
                var start = $(window).scrollTop();
                var length = Math.abs(end - start);
                var scrollPercentage = 0;
                var isBroadcasted = false;

                $('html, body').stop().animate({
                    scrollTop: end
                }, {
                        duration: 1000,
                        step: function (now, fx) {
                            scrollPercentage = (Math.round(((Math.abs($(window).scrollTop() - start)) / length) * 100000)) / 1000;

                            if (scrollPercentage > 80 && !isBroadcasted) {
                                $rootScope.$broadcast('playacc_scroll_complete', h);
                                isBroadcasted = true;
                            }
                        }
                    })
                    .promise().then(
                        function () { }
                    );
            }
        }
    };


    //
    playacc.resizeVideo = function ($context, $video) {
        var videoWidthOrg = 0;
        var videoHeightOrg = 0;

        $video[0].load();
        $video[0].addEventListener('loadedmetadata', function (e) {
            onVideoLoaded(e, $context, this);
        }, false);

        function onVideoLoaded(event, $context, video) {
            videoWidthOrg = video.videoWidth;
            videoHeightOrg = video.videoHeight;

            $(window).on('resize', onWindowResize).trigger('resize');
        }

        function onWindowResize(e) {
            resizeVideo();
        }

        function resizeVideo() {
            var dimensions = UtilsService.getFullscreenImageDimensions({ image: $video, context: $context, orgwidth: videoWidthOrg, orgheight: videoHeightOrg });
            $video.css({
                'width': (dimensions.width + 'px'),
                'height': (dimensions.height + 'px'),
                'margin-left': (dimensions.marginLeft + 'px'),
                'margin-top': (dimensions.marginTop + 'px')
            });
        }
    };

    return playacc;
}]);
