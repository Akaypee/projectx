app.controller('beo-reservation-module-ctrl', ['$scope', 'beoReserveService', '$element', function ($scope, beoReserveService, $element) {
    $scope.productID = '';
    $scope.FilterID = '';
    $scope.FormID = '';
    $scope.activeViewId = 0;
    $scope.isFormReady = false;
    $scope.isSubmitting = false;
    $scope.submitError = false;
    $scope.isFormSubmitted = false;
    $scope.isStoresSelectDisabled = true;
    $scope.data = { Options: {} };
    $scope.cities = [];
    $scope.stores = [];
    $scope.store = {};
    $scope.optionsView = false;
    $scope.submitBtnCtaTxt = '';

    $scope.$watch('store', function (n, o) {
        if (n) {
            $scope.data.Store = n.ID;
        } else {
            $scope.data.Store = '';
        }
    });

    // setting initial product type
    $scope.onProductInit = function (id) {
        $scope.productID = id;
        if (!$scope.data.ProductID) {
            $scope.data.ProductID = id;
        }
    };

    $scope.onFilterIdInit = function (id) {
        $scope.FilterID = id;
        if (!$scope.data.FilterID) {
            $scope.data.FilterID = id;
        }
    };

    $scope.onFormIdInit = function (id) {
        $scope.FormID = id;
        if (!$scope.data.FormID) {
            $scope.data.FormID = id;
        }
    };

    $scope.isStoresSelectDisabled = function () {
        return stores.length === 0 || isSubmitting || isStoresSelectDisabled;
    };

    // yes, this IS a hack!
    // hooking prerendered cities select dropdown up with angular
    // not doing this will throw an error the first time you select 
    // an option from the cities dropdown without changing the preselected country
    $scope.onCitiesInit = function () {
        var tmparr = [];
        $element.find('#rsrvtn_city option').each(function (i) {
            tmparr.push($(this).text());
        });
        
        $scope.cities = tmparr;

        $element.find('#rsrvtn_city').trigger('chosen:updated');
    };

    // form submit
    $scope.submit = function (event) {
        beoReserveService.submit($scope.data).then(onSubmitSucces, onSubmitError);
        $scope.isSubmitting = true;
    };

    function onSubmitSucces() {
        $scope.$broadcast('form_submit_success');
    }

    function onSubmitError() {
        $scope.$broadcast('form_submit_error');
    }
}]);

/// <reference path="../../Vendor/Angular/angular.min.js" />
/// <reference path="../../Vendor/Jquery/jquery-1.11.3.js" />
app.controller('beo-parallax-container-ctrl', ['$scope', '$element', '$attrs', 'UtilsService', function ($scope, $element, $attrs, UtilsService) {
    $scope.overflowDefault = 1;
    $scope.overflow = $attrs.overflow ? parseInt($attrs.overflow, 10) / 100 : $scope.overflowDefault;
    $scope.overflowOnEnter = $attrs.overflowOnEnter ? UtilsService.stringToBool($attrs.overflowOnEnter) : false;
    $scope.overflowOnExit = $attrs.overflowOnExit ? UtilsService.stringToBool($attrs.overflowOnExit) : false;
    $scope.windowWidthThreshold = $attrs.parallaxWinwidth ? parseInt($attrs.parallaxWinwidth, 10) : 0;

    $scope.overflow = $scope.overflow !== $scope.overflow ? $scope.overflowDefault : $scope.overflow;

    $scope.container = $element;
}]);
